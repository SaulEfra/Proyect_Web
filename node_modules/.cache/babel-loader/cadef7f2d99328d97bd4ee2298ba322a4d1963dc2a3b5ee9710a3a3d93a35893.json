labelledby=\"NuevaVentaLabel\" aria-hidden=\"true\">\n        <div class=\"modal-dialog modal-lg\">\n          <div class=\"modal-content\">\n            <div class=\"modal-header\">\n              <h5 class=\"modal-title\" id=\"agregarproductoLabel\">Nueva Venta</h5>\n              <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n            </div>\n            <div class=\"modal-body\">\n              <NuevaVenta />\n            </div>\n          </div>\n        </div>\n      </div>\n\n    </div>\n\n    <div class=\"movements\">\n      <div class=\"tab-buttons\">\n        <button @click=\"showSection('transacciones')\">Transacciones</button>\n        <button @click=\"showSection('cierresCaja')\">Cierres de caja</button>\n      </div>\n\n      <div class=\"transaction-section\" v-if=\"currentSection === 'transacciones'\">\n        <div>\n          <div class=\"filters\">\n            <select>\n              <option>Diario</option>\n              <option>Semanal</option>\n              <option>Mensual</option>\n            </select>\n            <input type=\"date\" />\n            <input type=\"text\" placeholder=\"Buscar concepto...\" />\n            <button>Buscar</button>\n\n          </div>\n        </div>\n\n        <!-- Tabla de Transacciones -->\n        <div class=\"transaction-table\">\n          <div>\n            <div class=\"balances\">\n              <div class=\"balance\">Balance: $500</div>\n              <div class=\"sales\">Ventas totales: $500</div>\n              <div class=\"expenses\">Gastos totales: $0</div>\n            </div>\n\n            <table class=\"transactions\">\n              <thead>\n                <tr>\n                  <th>Concepto</th>\n                  <th>Valor</th>\n                  <th>Medio de pago</th>\n                  <th>Fecha y hora</th>\n                  <th>Estado</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr>\n                  <td>Producto1</td>\n                  <td>$500</td>\n                  <td>Efectivo</td>\n                  <td>24/05/2024 | 6:16 pm</td>\n                  <td>Pagada</td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"cierres-caja-section\" v-if=\"currentSection === 'cierresCaja'\">\n        <!-- Sin contenido para Cierres de caja aún -->\n        <div>\n          <div class=\"filters\">\n            <select>\n              <option>Diario</option>\n              <option>Semanal</option>\n              <option>Mensual</option>\n            </select>\n            <input type=\"date\" />\n            <input type=\"text\" placeholder=\"Buscar concepto...\" />\n            <button>Buscar</button>\n          </div>\n        </div>\n        <div class=\"cierres-caja-table\">\n          <div>\n            <table class=\"cierres\">\n              <thead>\n                <tr>\n                  <th>Fecha</th>\n                  <th>Hora de Apertura</th>\n                  <th>Hora de Cierre</th>\n                  <th>Ventas Totales</th>\n                  <th>Gastos Totales</th>\n                  <th>Balance Final</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr>\n                  <td>24/05/2024</td>\n                  <td>8:00 am</td>\n                  <td>6:00 pm</td>\n                  <td>$1500</td>\n                  <td>$500</td>\n                  <td>$1000</td>\n                </tr>\n                <tr>\n                  <td>25/05/2024</td>\n                  <td>8:00 am</td>\n                  <td>6:00 pm</td>\n                  <td>$2000</td>\n                  <td>$300</td>\n                  <td>$1700</td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n\n\n\n\n      </div>\n    </div>\n  </div>\n\n</template>\n\n<script>\nimport NuevaVenta f