toFinal: '',\r\n      fechaHoraCierre: new Date().toISOString().slice(0, 16),\r\n      datosCajaAbierta: null,\r\n      ventasTotales: 0,\r\n      gastosTotales: 0,\r\n      balanceTotal: 0\r\n    };\r\n  },\r\n  mounted() {\r\n    this.obtenerCajaAbierta();\r\n  },\r\n  methods: {\r\n    async obtenerCajaAbierta() {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/cajaabierta');\r\n        this.datosCajaAbierta = response.data;\r\n        this.ventasTotales = await this.obtenerVentasTotales(this.datosCajaAbierta.IDAbrirCaja);\r\n        this.gastosTotales = await this.obtenerGastosTotales(this.datosCajaAbierta.IDAbrirCaja);\r\n        this.balanceTotal = parseFloat(this.datosCajaAbierta.MontoInicial) + this.ventasTotales - this.gastosTotales;\r\n      } catch (error) {\r\n        console.error('Error al obtener la caja abierta:', error);\r\n      }\r\n    },\r\n    async obtenerVentasTotales(idAbrirCaja) {\r\n      try {\r\n        const response = await axios.get(`http://localhost:3000/ventasTotal/${idAbrirCaja}`);\r\n        return response.data.VentasTotal;\r\n      } catch (error) {\r\n        console.error('Error al obtener ventas totales:', error);\r\n        return 0;\r\n      }\r\n    },\r\n    async obtenerGastosTotales(idAbrirCaja) {\r\n      try {\r\n        const response = await axios.get(`http://localhost:3000/gastosTotal/${idAbrirCaja}`);\r\n        return response.data.GastosTotal;\r\n      } catch (error) {\r\n        console.error('Error al obtener gastos totales:', error);\r\n        return 0;\r\n      }\r\n    },\r\n    async MontoFinalCaja() {\r\n      const formData = {\r\n        montoFinal: this.montoFinal,\r\n        fechaHoraCierre: this.fechaHoraCierre\r\n      };\r\n      try {\r\n        const response = await axios.post('http://localhost:3000/cierrecaja', formData);\r\n        console.log('Respuesta del servidor:', response.data);\r\n        alert('Se cerró caja correctamente');\r\n        this.limpiarDatos(); // Limpiar datos\r\n      } catch (error) {\r\n        console.error('Error al cerrar caja:', error);\r\n        alert('Error al cerrar caja: ' + error.message);\r\n      }\r\n    },\r\n    limpiarDatos() {\r\n      this.montoFinal = '';\r\n    }\r\n  }\r\n};\r\n</script>\r\n  \r\n \r\n  \r\n  <style scoped>\r\n  .form-group {\r\n    margin-bottom: 15px;\r\n  }\r\n  \r\n  .btn-primary {\r\n    background-color: #007bff;\r\n    border-color: #007bff;\r\n    text-align: end;\r\n  }\r\n  \r\n  .btn-primary:hover {\r\n    background-color: #0056b3;\r\n    border-color: #0056b3;\r\n  }\r\n  </style>\r\n  "],"mappings":"AA4BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,eAAe,EAAE,IAAIC,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,CAAC,EAAE,EAAE,CAAC;MACtDC,gBAAgB,EAAE,IAAI;MACtBC,aAAa,EAAE,CAAC;MAChBC,aAAa,EAAE,CAAC;MAChBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,kBAAkB,CAAC,CAAC;EAC3B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,kBAAkBA,CAAA,EAAG;MACzB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,mCAAmC,CAAC;QACrE,IAAI,CAACR,gBAAe,GAAIO,QAAQ,CAACb,IAAI;QACrC,IAAI,CAACO,aAAY,GAAI,MAAM,IAAI,CAACQ,oBAAoB,CAAC,IAAI,CAACT,gBAAgB,CAACU,WAAW,CAAC;QACvF,IAAI,CAACR,aAAY,GAAI,MAAM,IAAI,CAACS,oBAAoB,CAAC,IAAI,CAACX,gBAAgB,CAACU,WAAW,CAAC;QACvF,IAAI,CAACP,YAAW,GAAIS,UAAU,CAAC,IAAI,CAACZ,gBAAgB,CAACa,YAAY,IAAI,IAAI,CAACZ,aAAY,GAAI,IAAI,CAACC,aAAa;MAC9G,EAAE,OAAOY,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,mCAAmC,EAAEA,KAAK,CAAC;MAC3D;IACF,CAAC;IACD,MAAML,oBAAoBA,CAACO,WAAW,EAAE;MACtC,IAAI;QACF,MAAMT,QAAO,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,qCAAqCQ,WAAW,EAAE,CAAC;QACpF,OAAOT,QAAQ,CAACb,IAAI,CAACuB,WAAW;MAClC,EAAE,OAAOH,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,kCAAkC,EAAEA,KAAK,CAAC;QACxD,OAAO,CAAC;MACV;IACF,CAAC;IACD,MAAMH,oBAAoBA,CAACK,WAAW,EAAE;MACtC,IAAI;QACF,MAAMT,QAAO,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,qCAAqCQ,WAAW,EAAE,CAAC;QACpF,OAAOT,QAAQ,CAACb,IAAI,CAACwB,WAAW;MAClC,EAAE,OAAOJ,KAAK,EAA