{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'CerrarCaja',\n  data() {\n    return {\n      montoFinal: '',\n      fechaHoraCierre: new Date().toISOString().slice(0, 16),\n      datosCajaAbierta: null,\n      ventasTotales: 0,\n      gastosTotales: 0,\n      balanceTotal: 0\n    };\n  },\n  mounted() {\n    this.obtenerCajaAbierta();\n  },\n  methods: {\n    async obtenerCajaAbierta() {\n      try {\n        const response = await axios.get('http://localhost:3000/cajaabierta');\n        this.datosCajaAbierta = response.data;\n        this.ventasTotales = await this.obtenerVentasTotales(this.datosCajaAbierta.IDAbrirCaja);\n        this.gastosTotales = await this.obtenerGastosTotales(this.datosCajaAbierta.IDAbrirCaja);\n        this.balanceTotal = parseFloat(this.datosCajaAbierta.MontoInicial) + this.ventasTotales - this.gastosTotales;\n      } catch (error) {\n        console.error('Error al obtener la caja abierta:', error);\n      }\n    },\n    async obtenerVentasTotales(idAbrirCaja) {\n      try {\n        const response = await axios.get(`http://localhost:3000/ventasTotal/${idAbrirCaja}`);\n        return response.data.VentasTotal;\n      } catch (error) {\n        console.error('Error al obtener ventas totales:', error);\n        return 0;\n      }\n    },\n    async obtenerGastosTotales(idAbrirCaja) {\n      try {\n        const response = await axios.get(`http://localhost:3000/gastosTotal/${idAbrirCaja}`);\n        return response.data.GastosTotal;\n      } catch (error) {\n        console.error('Error al obtener gastos totales:', error);\n        return 0;\n      }\n    },\n    async MontoFinalCaja() {\n      const formData = {\n        montoFinal: this.montoFinal,\n        fechaHoraCierre: this.fechaHoraCierre\n      };\n      try {\n        const response = await axios.post('http://localhost:3000/cierrecaja', formData);\n        console.log('Respuesta del servidor:', response.data);\n        alert('Se cerró caja correctamente')