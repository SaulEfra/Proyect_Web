ole.error('Error al crear la categoría:', error);\n      }\n    },\n    async createCantidadProduct() {\n      const formData = {\n        CantidadProduc: this.CantidadProduc\n      };\n      try {\n        await axios.post('http://localhost:3000/cantidadproduc', formData);\n        alert('Cantidad creada con éxito');\n        this.CantidadProduc = \"\";\n      } catch (error) {\n        console.error('Error al crear la cantidad:', error);\n      }\n    },\n    async Productoscount() {\n      try {\n        const response = await axios.get('http://localhost:3000/Productocont');\n        this.datosprodcount = response.data.results;\n      } catch (error) {\n        console.error('Error al obtener la cantidad productos:', error);\n        alert('Error al obtener la cantidad productos');\n      }\n    },\n    async ProductosPreciocount() {\n      try {\n        const response = await axios.get('http://localhost:3000/Productocontinventario');\n        this.datosprodPreciocount = response.data.results;\n      } catch (error) {\n        console.error('Error al obtener los precios:', error);\n        alert('Error al obtener los precios');\n      }\n    },\n    async eliminarProducto(idProducto) {\n      try {\n        await axios.delete(`http://localhost:3000/productoselim/${idProducto}`);\n        await this.loadProducts();\n      } catch (error) {\n        console.error('Error al eliminar el producto:', error);\n        alert('Error al eliminar el producto');\n      }\n    },\n    actualiProductprueb(idProducto) {\n      this.idparaact = idProducto;\n    }\n  }\n};","map":{"version":3,"names":["CardProduct","ModalAgregarProducto","ModalActualizarProducto","ModalCrearCategoria","ModalCrearCantidad","ParteLateral","axios","name","components","data","searchQuery","categoriaName","CantidadProduc","datosprod","datosprodcount","datosprodPreciocount","idparaact","computed","filteredProducts","lowerCaseQuery","toLowerCase","filter","product","Nombre","includes","Descripcion","mo