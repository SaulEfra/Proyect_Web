{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'InicioSesion',\n  data() {\n    return {\n      email: '',\n      password: '',\n      showPassword: false\n    };\n  },\n  methods: {\n    async login() {\n      if (!this.email || !this.password) {\n        alert('Por favor, ingresa tu usuario y contraseña.');\n        return;\n      }\n      try {\n        const response = await axios.post('http://localhost:3000/login', {\n          email: this.email,\n          password: this.password\n        }, {\n          withCredentials: true\n        });\n        if (response.status === 200) {\n          this.$router.push('/Movimientos');\n        }\n      } catch (error) {\n        if (error.response) {\n          alert('Error al iniciar sesión: ' + error.response.data);\n        } else if (error.request) {\n          alert('Error al iniciar sesión: No hay respuesta del servidor');\n        } else {\n          alert('Error al iniciar sesión: ' + error.message);\n        }\n      }\n    },\n    togglePassword() {\n      this.showPassword = !this.showPassword;\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","showPassword","methods","login","alert","response","post","withCredentials","status","$router","push","error","request","message","togglePassword"],"sources":["C:\\Users\\R\\Documents\\Tareas 9cuatri\\Proyecto\\Proyect_Web\\src\\components\\InicioSesion.vue"],"sourcesContent":["<template>\r\n  <div class=\"login\">\r\n    <div class=\"login-container\">\r\n      <h1 class=\"title\">Inicio de sesión</h1>\r\n      <form @submit.prevent=\"login\">\r\n        <div class=\"form-group\">\r\n          <label for=\"user\" class=\"form-label\">Usuario</label>\r\n          <input class=\"form-control\" id=\"user\" v-model=\"email\" type=\"text\" placeholder=\"Ingresa tu usuario\">\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"pass\" class=\"form-label\">Contraseña</label>\r\n          <div class=\"password-input\">\r\n            <input class=\"form-control\" id=\"pass\" v-model=\"password\" :type=\"showPassword ? 'text' : 'password'\" placeholder=\"Ingresa tu contraseña\">\r\n            <button type=\"button\" class=\"toggle-password\" @click=\"togglePassword\">\r\n              <i :class=\"showPassword ? 'fas fa-eye-slash' : 'fas fa-eye'\"></i>\r\n            </button>\r\n          </div>\r\n        </div>\r\n        <button class=\"login-button\" type=\"submit\">Comenzar</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'InicioSesion',\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n      showPassword: false\r\n    };\r\n  },\r\n  methods: {\r\n    async login() {\r\n      if (!this.email || !this.password) {\r\n        alert('Por favor, ingresa tu usuario y contraseña.');\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const response = await axios.post('http://localhost:3000/login', {\r\n          email: this.email,\r\n          password: this.password\r\n        }, { withCredentials: true });\r\n\r\n        if (response.status === 200) {\r\n          this.$router.push('/Movimientos');\r\n        }\r\n      } catch (error) {\r\n        if (error.response) {\r\n          alert('Error al iniciar sesión: ' + error.response.data);\r\n        } else if (error.request) {\r\n          alert('Error al iniciar sesión: No hay respuesta del servidor');\r\n        } else {\r\n          alert('Error al iniciar sesión: ' + error.message);\r\n        }\r\n      }\r\n    },\r\n    togglePassword() {\r\n      this.showPassword = !this.showPassword;\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100vh;\r\n  background: linear-gradient(135deg, #1A365D, #2C5282);\r\n  