{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'InicioSesion',\n  data() {\n    return {\n      email: '',\n      password: ''\n    };\n  },\n  methods: {\n    async login() {\n      if (!this.email || !this.password) {\n        alert('Por favor, ingresa tu usuario y contraseña.');\n        return;\n      }\n      try {\n        const response = await axios.post('http://localhost:3000/login', {\n          email: this.email,\n          password: this.password\n        }, {\n          withCredentials: true\n        });\n        if (response.status === 200) {\n          this.$router.push('/Movimientos'); // Redirige al componente Movimientos\n        }\n      } catch (error) {\n        if (error.response) {\n          alert('Error al iniciar sesión: ' + error.response.data);\n        } else if (error.request) {\n          alert('Error al iniciar sesión: No hay respuesta del servidor');\n        } else {\n          alert('Error al iniciar sesión: ' + error.message);\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","methods","login","alert","response","post","withCredentials","status","$router","push","error","request","message"],"sources":["C:\\Users\\R\\Documents\\Tareas 9cuatri\\Proyecto\\Proyect_Web\\src\\components\\InicioSesion.vue"],"sourcesContent":["<template>\r\n  <div class=\"login\">\r\n    <div class=\"login-container\">\r\n      <h1 class=\"title\">Inicio de sesión</h1>\r\n      <form @submit.prevent=\"login\">\r\n        <div class=\"form-group\">\r\n          <label for=\"user\" class=\"form-label\">Usuario</label>\r\n          <input class=\"form-control\" id=\"user\" v-model=\"email\" type=\"text\" placeholder=\"Ingresa tu usuario\">\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"pass\" class=\"form-label\">Contraseña</label>\r\n          <input class=\"form-control\" id=\"pass\" v-model=\"password\" type=\"password\" placeholder=\"Ingresa tu contraseña\">\r\n        </div>\r\n        <button class=\"login-button\" type=\"submit\">Comenzar</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'InicioSesion',\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n    };\r\n  },\r\n  methods: {\r\n    async login() {\r\n      if (!this.email || !this.password) {\r\n        alert('Por favor, ingresa tu usuario y contraseña.');\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const response = await axios.post('http://localhost:3000/login', {\r\n          email: this.email,\r\n          password: this.password\r\n        }, { withCredentials: true });\r\n\r\n        if (response.status === 200) {\r\n          \r\n          this.$router.push('/Movimientos'); // Redirige al componente Movimientos\r\n        }\r\n      } catch (error) {\r\n        if (error.response) {\r\n          alert('Error al iniciar sesión: ' + error.response.data);\r\n        } else if (error.request) {\r\n          alert('Error al iniciar sesión: No hay respuesta del servidor');\r\n        } else {\r\n          alert('Error al iniciar sesión: ' + error.message);\r\n        }\r\n      }\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100vh;\r\n  background: linear-gradient(135deg, #34495E, #2C3E50);\r\n  animation: gradientBG 15s ease infinite;\r\n  background-size: 400% 400%;\r\n}\r\n\r\n@keyframes gradientBG {\r\n  0% { background-position: 0% 50%; }\r\n  50% { background-position: 100% 50%; }\r\n  100% { background-position: 0% 50%; }\r\n}\r\n\r\n.login-container {\r\n  background-color: #ECF0F1;\r\n  padding: 2rem;\r\n  border-radius: 8px;\r\n  box-shadow: 0 0 20px rgba(0, 0, 0, 0.2);\r\n  width: 100%;\r\n  max-width: 400px;\r\n  animation: fadeIn 1s ease-out;\r\n}\r\n\r\n@keyframes fadeIn {\r\n  from { opacity: 0; transform: translateY(-20px); }\r\n  to { opacity: 1; transform: translateY(0); }\r\n}\r\n\r\n.title {\r\n  color: #2C3E50;\r\n  text-align: center;\r\n  margin-bottom: 2rem;\r\n  font-size: 2rem;\r\n  animation: slideDown 0.5s ease-out;\r\n}\r\n\r\n@keyframes slideDown {\r\n  from { opacity: 0; transform: translateY(-20px); }\r\n  to { opacity: 1; transform: translateY(0); }\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 1.5rem;\r\n}\r\n\r\n.form-label {\r\n  display: block;\r\n  margin-bottom: 0.5rem;\r\n  color: #34495E;\r\n  font-weight: bold;\r\n  transition: color 0.3s ease;\r\n}\r\n\r\n.form-control {\r\n  width: 100%;\r\n  padding: 0.75rem;\r\n  border: 1px solid #BDC3C7;\r\n  border-radius: 4px;\r\n  font-size: 1rem;\r\n  transition: all 0.3s ease;\r\n  background-color: #FFFFFF;\r\n}\r\n\r\n.form-control:focus {\r\n  outline: none;\r\n  border-color: #3498DB;\r\n  box-shadow: 0 0 0 2px rgba(52, 152, 219, 0.2);\r\n}\r\n\r\n.login-button {\r\n  width: 100%;\r\n  padding: 0.75rem;\r\n  background-color: #27AE60;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  font-size: 1rem;\r\n  cursor: pointer;\r\n  transition: all 0.3s ease;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.login-button:hover {\r\n  background-color: #2ECC71;\r\n}\r\n\r\n.login-button:active {\r\n  background-color: #27AE60;\r\n}\r\n\r\n.login-button::after {\r\n  content: '';\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  width: 5px;\r\n  height: 5px;\r\n  background: rgba(255, 255, 255, 0.5);\r\n  opacity: 0;\r\n  border-radius: 100%;\r\n  transform: scale(1, 1) translate(-50%);\r\n  transform-origin: 50% 50%;\r\n}\r\n\r\n.login-button:focus:not(:active)::after {\r\n  animation: ripple 1s ease-out;\r\n}\r\n\r\n@keyframes ripple {\r\n  0% { transform: scale(0, 0); opacity: 1; }\r\n  20% { transform: scale(25, 25); opacity: 1; }\r\n  100% { opacity: 0; transform: scale(40, 40); }\r\n}\r\n</style>"],"mappings":";AAoBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI,CAAC,IAAI,CAACH,KAAI,IAAK,CAAC,IAAI,CAACC,QAAQ,EAAE;QACjCG,KAAK,CAAC,6CAA6C,CAAC;QACpD;MACF;MAEA,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMR,KAAK,CAACS,IAAI,CAAC,6BAA6B,EAAE;UAC/DN,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,EAAE;UAAEM,eAAe,EAAE;QAAK,CAAC,CAAC;QAE7B,IAAIF,QAAQ,CAACG,MAAK,KAAM,GAAG,EAAE;UAE3B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,cAAc,CAAC,EAAE;QACrC;MACF,EAAE,OAAOC,KAAK,EAAE;QACd,IAAIA,KAAK,CAACN,QAAQ,EAAE;UAClBD,KAAK,CAAC,2BAA0B,GAAIO,KAAK,CAACN,QAAQ,CAACN,IAAI,CAAC;QAC1D,OAAO,IAAIY,KAAK,CAACC,OAAO,EAAE;UACxBR,KAAK,CAAC,wDAAwD,CAAC;QACjE,OAAO;UACLA,KAAK,CAAC,2BAA0B,GAAIO,KAAK,CAACE,OAAO,CAAC;QACpD;MACF;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}