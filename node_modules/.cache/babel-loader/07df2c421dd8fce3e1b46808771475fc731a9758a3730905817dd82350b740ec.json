{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Swal from 'sweetalert2';\nimport axios from 'axios';\nimport ParteLateral from '../components/ParteLateral.vue';\nexport default {\n  name: 'ProveedoresApp',\n  components: {\n    ParteLateral\n  },\n  data() {\n    return {\n      datosempl: [],\n      mostrarFormulario: false,\n      proveedores: [],\n      nuevoProveedor: {\n        IDProveedor: null,\n        Nombre: '',\n        Telefono: '',\n        Adeudo: 0,\n        activo: true\n      },\n      modoEdicion: false,\n      busqueda: '',\n      proveedorSeleccionado: null,\n      modalTitulo: '',\n      cantidadAdeudoAbono: 0,\n      esAdeudo: true,\n      mostrarModal: false\n    };\n  },\n  computed: {\n    proveedoresFiltrados() {\n      if (this.busqueda) {\n        return this.proveedores.filter(proveedor => proveedor.Nombre.toLowerCase().includes(this.busqueda.toLowerCase()));\n      }\n      return this.proveedores;\n    }\n  },\n  methods: {\n    async agregarProveedor() {\n      const swalWithBootstrapButtons = Swal.mixin({\n        customClass: {\n          confirmButton: \"btn btn-success\",\n          cancelButton: \"btn btn-danger\"\n        },\n        buttonsStyling: false\n      });\n      if (this.modoEdicion) {\n        swalWithBootstrapButtons.fire({\n          title: \"¿Actualizar proveedor?\",\n          text: \"¿Está seguro de que desea actualizar este proveedor?\",\n          icon: \"question\",\n          showCancelButton: true,\n          confirmButtonText: \"Sí, actualizar\",\n          cancelButtonText: \"Cancelar\",\n          reverseButtons: true\n        }).then(async result => {\n          if (result.isConfirmed) {\n            const index = this.proveedores.findIndex(prov => prov.IDProveedor === this.nuevoProveedor.IDProveedor);\n            await axios.put('http://localhost:3000/Neg/proveedoresAct', {\n              IDProveedor: this.nuevoProveedor.IDProveedor,\n              Nombre: this.nuevoProveedor.Nombre,\n              Telefono: this.nuevoProveedor.Telefono,\n              Adeudo: this.nuevoProveedor.Adeudo,\n              activo: this.nuevoProveedor.activo\n            });\n            this.resetearFormulario();\n            this.obtenerProveedores();\n            if (index !== -1) {\n              this.datosempl[index] = {\n                ...this.nuevoProveedor\n              };\n              this.resetearFormulario();\n              Swal.fire({\n                title: \"Proveedor actualizado\",\n                icon: \"success\"\n              });\n            }\n          }\n        });\n      } else {\n        const nuevoId = Date.now().toString();\n        this.proveedores.push({\n          ...this.nuevoProveedor,\n          IDProveedor: nuevoId\n        });\n        await axios.post('http://localhost:3000/Neg/proveedores', {\n          IDProveedor: this.nuevoProveedor.IDProveedor,\n          Nombre: this.nuevoProveedor.Nombre,\n          Telefono: this.nuevoProveedor.Telefono,\n          Adeudo: this.nuevoProveedor.Adeudo,\n          activo: this.nuevoProveedor.activo\n        });\n        this.resetearFormulario();\n        this.obtenerProveedores();\n        Swal.fire({\n          title: \"Proveedor añadido\",\n          icon: \"success\"\n        });\n      }\n    },\n    editarProveedor(proveedor) {\n      this.nuevoProveedor = {\n        ...proveedor\n      };\n      this.validarNombre();\n      this.modoEdicion = true;\n      this.mostrarFormulario = true;\n    },\n    async borrarProveedor(IDProveedor) {\n      const swalWithBootstrapButtons = Swal.mixin({\n        customClass: {\n          confirmButton: \"btn btn-success\",\n          cancelButton: \"btn btn-danger\"\n        },\n        buttonsStyling: false\n      });\n      swalWithBootstrapButtons.fire({\n        title: \"¿Está seguro?\",\n        text: \"No podrá revertir esta acción!\",\n        icon: \"warning\",\n        showCancelButton: true,\n        confirmButtonText: \"Sí, eliminar!\",\n        cancelButtonText: \"No, cancelar!\",\n        reverseButtons: true\n      }).then(async result => {\n        if (result.isConfirmed) {\n          try {\n            this.datosempl = this.datosempl.filter(prov => prov.IDProveedor !== IDProveedor);\n            await axios.delete(`http://localhost:3000/Neg/proveedores/${IDProveedor}`);\n          } catch (error) {\n            console.error(\"Error al eliminar el proveedor:\", error);\n          }\n          this.obtenerProveedores();\n          swalWithBootstrapButtons.fire({\n            title: \"Eliminado!\",\n            text: \"El proveedor ha sido eliminado.\",\n            icon: \"success\"\n          });\n        }\n      });\n    },\n    resetearFormulario() {\n      this.nuevoProveedor = {\n        IDProveedor: null,\n        Nombre: '',\n        Telefono: '',\n        Adeudo: 0,\n        activo: true\n      };\n      this.mostrarFormulario = false;\n      this.modoEdicion = false;\n    },\n    cancelarEdicion() {\n      this.resetearFormulario();\n    },\n    validarTelefono(event) {\n      this.nuevoProveedor.Telefono = event.target.value.replace(/[^0-9]/g, '');\n    },\n    validarNombre() {\n      this.nuevoProveedor.Nombre = this.nuevoProveedor.Nombre.trimStart();\n    },\n    buscarProveedor() {\n      // La búsqueda se realiza automáticamente gracias al computed property\n    },\n    mostrarModalAdeudo(proveedor) {\n      this.proveedorSeleccionado = proveedor;\n      this.modalTitulo = 'Añadir Adeudo';\n      this.esAdeudo = true;\n      this.cantidadAdeudoAbono = 0;\n      this.mostrarModal = true;\n    },\n    mostrarModalAbono(proveedor) {\n      this.proveedorSeleccionado = proveedor;\n      this.modalTitulo = 'Añadir Abono';\n      this.esAdeudo = false;\n      this.cantidadAdeudoAbono = 0;\n      this.mostrarModal = true;\n    },\n    procesarAdeudoAbono() {\n      if (this.proveedorSeleccionado) {\n        if (this.esAdeudo) {\n          this.proveedorSeleccionado.Adeudo += this.cantidadAdeudoAbono;\n        } else {\n          if (this.cantidadAdeudoAbono > this.proveedorSeleccionado.Adeudo) {\n            Swal.fire({\n              title: \"Error\",\n              text: \"El abono no puede ser mayor que el adeudo actual.\",\n              icon: \"error\"\n            });\n            return;\n          }\n          this.proveedorSeleccionado.Adeudo -= this.cantidadAdeudoAbono;\n        }\n        this.actualizarProveedor(this.proveedorSeleccionado);\n        this.cerrarModal();\n        Swal.fire({\n          title: this.esAdeudo ? \"Adeudo añadido\" : \"Abono realizado\",\n          icon: \"success\"\n        });\n      }\n    },\n    actualizarProveedor(proveedor) {\n      const index = this.proveedores.findIndex(p => p.IDProveedor === proveedor.IDProveedor);\n      if (index !== -1) {\n        this.$set(this.proveedores, index, {\n          ...proveedor\n        });\n      }\n    },\n    cerrarModal() {\n      this.mostrarModal = false;\n    },\n    async obtenerProveedores() {\n      try {\n        const response = await axios.get('http://localhost:3000/Neg/proveedoresget');\n        this.datosempl = response.data.results;\n        this.proveedores = response.data.results;\n      } catch (error) {\n        console.error('Error al obtener los proveedores:', error);\n        alert('Error al obtener los proveedores');\n      }\n    }\n  },\n  mounted() {\n    this.obtenerProveedores();\n  }\n};","map":{"version":3,"names":["Swal","axios","ParteLateral","name","components","data","datosempl","mostrarFormulario","proveedores","nuevoProveedor","IDProveedor","Nombre","Telefono","Adeudo","activo","modoEdicion","busqueda","proveedorSeleccionado","modalTitulo","cantidadAdeudoAbono","esAdeudo","mostrarModal","computed","proveedoresFiltrados","filter","proveedor","toLowerCase","includes","methods","agregarProveedor","swalWithBootstrapButtons","mixin","customClass","confirmButton","cancelButton","buttonsStyling","fire","title","text","icon","s