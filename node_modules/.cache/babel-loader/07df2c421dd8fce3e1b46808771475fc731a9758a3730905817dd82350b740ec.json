\n            this.obtenerProveedores()\r\n            if (index !== -1) {\r\n              this.datosempl[index] = { ...this.nuevoProveedor };\r\n              this.resetearFormulario()\r\n              Swal.fire({\r\n                title: \"Proveedor actualizado\",\r\n                icon: \"success\",\r\n              });\r\n            }\r\n          }\r\n        });\r\n      } else {\r\n        const nuevoId = Date.now().toString()\r\n        this.proveedores.push({ ...this.nuevoProveedor, IDProveedor: nuevoId })\r\n        await axios.post('http://localhost:3000/Neg/proveedores', {\r\n          IDProveedor: this.nuevoProveedor.IDProveedor,\r\n          Nombre: this.nuevoProveedor.Nombre,\r\n          Telefono: this.nuevoProveedor.Telefono,\r\n          Adeudo: this.nuevoProveedor.Adeudo,\r\n          activo: this.nuevoProveedor.activo\r\n        })\r\n        this.resetearFormulario()\r\n        this.obtenerProveedores()\r\n        Swal.fire({\r\n          title: \"Proveedor añadido\",\r\n          icon: \"success\",\r\n        });\r\n      }\r\n    },\r\n    editarProveedor(proveedor) {\r\n      this.nuevoProveedor = { ...proveedor }\r\n      this.validarNombre();\r\n      this.modoEdicion = true\r\n      this.mostrarFormulario = true\r\n    },\r\n    async borrarProveedor(IDProveedor) {\r\n      const swalWithBootstrapButtons = Swal.mixin({\r\n        customClass: {\r\n          confirmButton: \"btn btn-success\",\r\n          cancelButton: \"btn btn-danger\"\r\n        },\r\n        buttonsStyling: false\r\n      });\r\n\r\n      swalWithBootstrapButtons.fire({\r\n        title: \"¿Está seguro?\",\r\n        text: \"No podrá revertir esta acción!\",\r\n        icon: \"warning\",\r\n        showCancelButton: true,\r\n        confirmButtonText: \"Sí, eliminar!\",\r\n        cancelButtonText: \"No, cancelar!\",\r\n        reverseButtons: true\r\n      }).then(async (result) => {\r\n        if (result.isConfirmed) {\r\n          try {\r\n            this.datos