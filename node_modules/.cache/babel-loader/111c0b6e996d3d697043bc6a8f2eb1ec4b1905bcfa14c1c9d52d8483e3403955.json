{"ast":null,"code":"export default {\n  props: {\n    isVisible: {\n      type: Boolean,\n      required: true\n    },\n    dineroBase: {\n      type: Number,\n      default: 100000\n    },\n    fechaHora: {\n      type: String,\n      default: ''\n    },\n    ventas: {\n      type: Number,\n      default: 6000\n    },\n    abonos: {\n      type: Number,\n      default: 0\n    },\n    gastos: {\n      type: Number,\n      default: 0\n    }\n  },\n  data() {\n    return {\n      tab: 'efectivo',\n      efectivo: 100000,\n      descuadre: 0\n    };\n  },\n  computed: {\n    balanceTotal() {\n      return this.dineroBase + this.ventas - this.abonos - this.gastos;\n    },\n    descuadre() {\n      return this.balanceTotal - this.efectivo;\n    }\n  },\n  methods: {\n    closeModal() {\n      this.$emit('close');\n    },\n    confirmarCierre() {\n      if (this.descuadre === 0) {\n        alert(\"Cierre confirmado correctamente\");\n        this.closeModal();\n      } else {\n        alert(\"Hay un descuadre, no se puede confirmar el cierre\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["props","isVisible","type","Boolean","required","dineroBase","Number","default","fechaHora","String","ventas","abonos","gastos","data","tab","efectivo","descuadre","computed","balanceTotal","methods","closeModal","$emit","confirmarCierre","alert"],"sources":["D:\\Vers_Git_Proyecto\\Proyect_Web\\src\\components\\CierreCaja.vue"],"sourcesContent":["<template>\r\n    <div v-if=\"isVisible\" class=\"modal\">\r\n      <div class=\"modal-content\">\r\n        <span class=\"close\" @click=\"closeModal\">&times;</span>\r\n        <h2>Cerrar Caja</h2>\r\n        <div>\r\n          <h3>Registros realizados</h3>\r\n          <div>\r\n            <button :class=\"{'active': tab === 'efectivo'}\" @click=\"tab = 'efectivo'\">Efectivo</button>\r\n            <button :class=\"{'active': tab === 'digitales'}\" @click=\"tab = 'digitales'\">Digitales</button>\r\n          </div>\r\n          <div v-if=\"tab === 'efectivo'\">\r\n            <h4>Resumen de caja</h4>\r\n            <p>Dinero base: $ {{ dineroBase }}</p>\r\n            <p>Ventas: $ {{ ventas }}</p>\r\n            <p>Abonos: $ {{ abonos }}</p>\r\n            <p>Gastos: $ {{ gastos }}</p>\r\n            <h4>Balance total: $ {{ balanceTotal }}</h4>\r\n          </div>\r\n        </div>\r\n        <div>\r\n          <label for=\"cash\">Cu√°nto dinero tienes en efectivo?</label>\r\n          <input type=\"number\" v-model=\"efectivo\" />\r\n        </div>\r\n        <div v-if=\"descuadre !== 0\" class=\"alert\">\r\n          Tienes un descuadre. Te faltan $ {{ descuadre }}\r\n        </div>\r\n        <button @click=\"confirmarCierre\">Confirmar cierre</button>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    props: {\r\n      isVisible: {\r\n        type: Boolean,\r\n        required: true\r\n      },\r\n      dineroBase: {\r\n        type: Number,\r\n        default: 100000\r\n      },\r\n      fechaHora: {\r\n        type: String,\r\n        default: ''\r\n      },\r\n      ventas: {\r\n        type: Number,\r\n        default: 6000\r\n      },\r\n      abonos: {\r\n        type: Number,\r\n        default: 0\r\n      },\r\n      gastos: {\r\n        type: Number,\r\n        default: 0\r\n      }\r\n    },\r\n    data() {\r\n      return {\r\n        tab: 'efectivo',\r\n        efectivo: 100000,\r\n        descuadre: 0\r\n      };\r\n    },\r\n    computed: {\r\n      balanceTotal() {\r\n        return this.dineroBase + this.ventas - this.abonos - this.gastos;\r\n      },\r\n      descuadre() {\r\n        return this.balanceTotal - this.efectivo;\r\n      }\r\n    },\r\n    methods: {\r\n      closeModal() {\r\n        this.$emit('close');\r\n      },\r\n      confirmarCierre() {\r\n        if (this.descuadre === 0) {\r\n          alert(\"Cierre confirmado correctamente\");\r\n          this.closeModal();\r\n        } else {\r\n          alert(\"Hay un descuadre, no se puede confirmar el cierre\");\r\n        }\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .modal {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    background: rgba(0, 0, 0, 0.5);\r\n  }\r\n  .modal-content {\r\n    background: white;\r\n    padding: 20px;\r\n    border-radius: 8px;\r\n    width: 300px;\r\n  }\r\n  .close {\r\n    position: absolute;\r\n    top: 10px;\r\n    right: 10px;\r\n    cursor: pointer;\r\n  }\r\n  button.active {\r\n    background-color: #ccc;\r\n  }\r\n  .alert {\r\n    color: red;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAiCE,eAAe;EACbA,KAAK,EAAE;IACLC,SAAS,EAAE;MACTC,IAAI,EAAEC,OAAO;MACbC,QAAQ,EAAE;IACZ,CAAC;IACDC,UAAU,EAAE;MACVH,IAAI,EAAEI,MAAM;MACZC,OAAO,EAAE;IACX,CAAC;IACDC,SAAS,EAAE;MACTN,IAAI,EAAEO,MAAM;MACZF,OAAO,EAAE;IACX,CAAC;IACDG,MAAM,EAAE;MACNR,IAAI,EAAEI,MAAM;MACZC,OAAO,EAAE;IACX,CAAC;IACDI,MAAM,EAAE;MACNT,IAAI,EAAEI,MAAM;MACZC,OAAO,EAAE;IACX,CAAC;IACDK,MAAM,EAAE;MACNV,IAAI,EAAEI,MAAM;MACZC,OAAO,EAAE;IACX;EACF,CAAC;EACDM,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,UAAU;MACfC,QAAQ,EAAE,MAAM;MAChBC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,YAAYA,CAAA,EAAG;MACb,OAAO,IAAI,CAACb,UAAS,GAAI,IAAI,CAACK,MAAK,GAAI,IAAI,CAACC,MAAK,GAAI,IAAI,CAACC,MAAM;IAClE,CAAC;IACDI,SAASA,CAAA,EAAG;MACV,OAAO,IAAI,CAACE,YAAW,GAAI,IAAI,CAACH,QAAQ;IAC1C;EACF,CAAC;EACDI,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,IAAI,CAACC,KAAK,CAAC,OAAO,CAAC;IACrB,CAAC;IACDC,eAAeA,CAAA,EAAG;MAChB,IAAI,IAAI,CAACN,SAAQ,KAAM,CAAC,EAAE;QACxBO,KAAK,CAAC,iCAAiC,CAAC;QACxC,IAAI,CAACH,UAAU,CAAC,CAAC;MACnB,OAAO;QACLG,KAAK,CAAC,mDAAmD,CAAC;MAC5D;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}