{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'NuevaVenta',\n  data() {\n    return {\n      currentSection: 'transacciones',\n      // Define la sección inicial\n      products: [],\n      // Array para almacenar los productos\n      searchQuery: '' // Variable para almacenar la consulta de búsqueda\n    };\n  },\n  computed: {\n    filteredProducts() {\n      if (this.searchQuery.trim() === '') {\n        return this.products;\n      }\n      return this.products.filter(product => product.Nombre.toLowerCase().includes(this.searchQuery.toLowerCase()));\n    }\n  },\n  methods: {\n    async fetchProducts() {\n      try {\n        const response = await axios.get('http://localhost:3000/Producto'); // URL del endpoint\n        this.products = response.data.results;\n      } catch (error) {\n        console.error('Error al obtener productos:', error);\n      }\n    },\n    async searchProduct() {\n      // La búsqueda ya se maneja a través de la propiedad computada\n    },\n    addProductToCart(product) {\n      // Lógica para agregar el producto al carrito\n      console.log('Producto agregado al carrito:', product);\n    },\n    printReceipt() {\n      console.log('Imprimiendo comprobante...');\n    },\n    downloadReceipt() {\n      console.log('Descargando comprobante...');\n    },\n    showSection(section) {\n      this.currentSection = section;\n    }\n  },\n  created() {\n    this.fetchProducts(); // Obtener productos cuando el componente se crea\n  }\n};","map":{"version":3,"names":["axios","name","data","currentSection","products","searchQuery","computed","filteredProducts","trim","filter","product","Nombre","toLowerCase","includes","methods","fetchProducts","response","get","results","error","console","searchProduct","addProductToCart","log","printReceipt","downloadReceipt","showSection","section","created"],"sources":["C:\\Proyecto\\Proyect_Web\\Proyect_Web\\Proyect_Web\\src\\components\\NuevaVenta.vue"],"sourcesContent":["<template>\r\n  <div class=\"container-fluid text-bg-light p-3\">\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-12\">\r\n        <div class=\"row\">\r\n          <!-- Navbar para búsqueda -->\r\n          <div class=\"col-lg-8 col-md-12 mb-4\">\r\n            <div class=\"row nav\">\r\n              <div class=\"col-lg-12\">\r\n                <nav class=\"navbar navbar-expand-lg bg-body-tertiary nav2\">\r\n                  <div class=\"container-fluid\">\r\n                    <div class=\"collapse navbar-collapse\" id=\"navbarSupportedContent\">\r\n                      <ul class=\"navbar-nav me-auto mb-2 col-lg-12\">\r\n                        <form class=\"d-flex\" @submit.prevent=\"searchProduct\">\r\n                          <input class=\"form-control me-2\" v-model=\"searchQuery\" type=\"search\" placeholder=\"Buscar Producto\" aria-label=\"Buscar\">\r\n                          <button class=\"btn btn-outline-success\" type=\"submit\" style=\"color: blue; border-color: blue;\">Buscar</button>\r\n                        </form>\r\n                      </ul>\r\n                    </div>\r\n                  </div>\r\n                </nav>\r\n              </div>\r\n              <!-- Tarjetas de productos -->\r\n              <div class=\"col-lg-12 mt-3\">\r\n                <div class=\"card card1\" v-for=\"product in filteredProducts\" :key=\"product.IDProducto\" style=\"width: 100%;\">\r\n                  <img :src=\"'data:image/jpeg;base64,' + product.Datos\" class=\"card-img-top\" alt=\"\">\r\n                  <div class=\"card-body\">\r\n                    <h5 class=\"card-title\">{{ product.Nombre }}</h5>\r\n                    <h5>Producto</h5>\r\n                    <h5>{{ product.PrecioVenta }}</h5>\r\n                    <button class=\"btn btn-primary\" type=\"button\" @click=\"addProductToCart(product)\">Agregar Producto</button>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Otras secciones de la página -->\r\n          <div class=\"col-lg-4 col-md-12\">\r\n            <div v-if=\"currentSection === 'transacciones'\">\r\n              <!-- Contenido de la sección de transacciones -->\r\n              <div class=\"container\">\r\n                <h3>Resumen de Venta</h3>\r\n                <button class=\"btn btn-primary\" @click=\"printReceipt\">Imprimir Comprobante</button>\r\n                <button class=\"btn btn-secondary\" @click=\"downloadReceipt\">Descargar Comprobante</button>\r\n              </div>\r\n            </div>\r\n\r\n            <!-- Agrega más secciones según sea necesario -->\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'NuevaVenta',\r\n  data() {\r\n    return {\r\n      currentSection: 'transacciones', // Define la sección inicial\r\n      products: [], // Array para almacenar los productos\r\n      searchQuery: '' // Variable para almacenar la consulta de búsqueda\r\n    };\r\n  },\r\n  computed: {\r\n    filteredProducts() {\r\n      if (this.searchQuery.trim() === '') {\r\n        return this.products;\r\n      }\r\n      return this.products.filter(product =>\r\n        product.Nombre.toLowerCase().includes(this.searchQuery.toLowerCase())\r\n      );\r\n    }\r\n  },\r\n  methods: {\r\n    async fetchProducts() {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/Producto'); // URL del endpoint\r\n        this.products = response.data.results;\r\n      } catch (error) {\r\n        console.error('Error al obtener productos:', error);\r\n      }\r\n    },\r\n    async searchProduct() {\r\n      // La búsqueda ya se maneja a través de la propiedad computada\r\n    },\r\n    addProductToCart(product) {\r\n      // Lógica para agregar el producto al carrito\r\n      console.log('Producto agregado al carrito:', product);\r\n    },\r\n    printReceipt() {\r\n      console.log('Imprimiendo comprobante...');\r\n    },\r\n    downloadReceipt() {\r\n      console.log('Descargando comprobante...');\r\n    },\r\n    showSection(section) {\r\n      this.currentSection = section;\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchProducts(); // Obtener productos cuando el componente se crea\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.nav2 {\r\n  margin-bottom: 20px;\r\n}\r\n.card1 {\r\n  margin-bottom: 20px;\r\n}\r\n</style>\r\n"],"mappings":"AAyDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,eAAe;MAAE;MACjCC,QAAQ,EAAE,EAAE;MAAE;MACdC,WAAW,EAAE,EAAC,CAAE;IAClB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB,IAAI,IAAI,CAACF,WAAW,CAACG,IAAI,CAAC,MAAM,EAAE,EAAE;QAClC,OAAO,IAAI,CAACJ,QAAQ;MACtB;MACA,OAAO,IAAI,CAACA,QAAQ,CAACK,MAAM,CAACC,OAAM,IAChCA,OAAO,CAACC,MAAM,CAACC,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACR,WAAW,CAACO,WAAW,CAAC,CAAC,CACtE,CAAC;IACH;EACF,CAAC;EACDE,OAAO,EAAE;IACP,MAAMC,aAAaA,CAAA,EAAG;MACpB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMhB,KAAK,CAACiB,GAAG,CAAC,gCAAgC,CAAC,EAAE;QACpE,IAAI,CAACb,QAAO,GAAIY,QAAQ,CAACd,IAAI,CAACgB,OAAO;MACvC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,6BAA6B,EAAEA,KAAK,CAAC;MACrD;IACF,CAAC;IACD,MAAME,aAAaA,CAAA,EAAG;MACpB;IAAA,CACD;IACDC,gBAAgBA,CAACZ,OAAO,EAAE;MACxB;MACAU,OAAO,CAACG,GAAG,CAAC,+BAA+B,EAAEb,OAAO,CAAC;IACvD,CAAC;IACDc,YAAYA,CAAA,EAAG;MACbJ,OAAO,CAACG,GAAG,CAAC,4BAA4B,CAAC;IAC3C,CAAC;IACDE,eAAeA,CAAA,EAAG;MAChBL,OAAO,CAACG,GAAG,CAAC,4BAA4B,CAAC;IAC3C,CAAC;IACDG,WAAWA,CAACC,OAAO,EAAE;MACnB,IAAI,CAACxB,cAAa,GAAIwB,OAAO;IAC/B;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACb,aAAa,CAAC,CAAC,EAAE;EACxB;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}