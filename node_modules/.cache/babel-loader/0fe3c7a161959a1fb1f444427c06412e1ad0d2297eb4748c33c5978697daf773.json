{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'EmpleadosApp',\n  data() {\n    return {\n      mostrarFormulario: false,\n      datosempl: [],\n      nuevoEmpleado: {\n        IDEmpleado: null,\n        Nombre: '',\n        NumeroDeTelefono: '',\n        Rol: '',\n        activo: true\n      },\n      modoEdicion: false,\n      opcionesCargo: ['Encargado', 'Empleado']\n    };\n  },\n  mounted() {\n    this.obtenerEmpleados();\n  },\n  methods: {\n    async agregarEmpleado() {\n      try {\n        if (this.modoEdicion) {\n          await axios.put('http://localhost:3000/Neg/empleadosAct', {\n            IDEmpleado: this.nuevoEmpleado.IDEmpleado,\n            Nombre: this.nuevoEmpleado.Nombre,\n            NumeroDeTelefono: this.nuevoEmpleado.NumeroDeTelefono,\n            Rol: this.nuevoEmpleado.Rol,\n            activo: this.nuevoEmpleado.activo\n          });\n          const index = this.datosempl.findIndex(emp => emp.IDEmpleado === this.nuevoEmpleado.IDEmpleado);\n          if (index !== -1) {\n            this.datosempl[index] = {\n              ...this.nuevoEmpleado\n            };\n          }\n          alert(\"datos enviados\");\n        } else {\n          this.nuevoEmpleado.IDEmpleado = Date.now();\n          await axios.post('http://localhost:3000/Neg/empleados', {\n            IDEmpleado: this.nuevoEmpleado.IDEmpleado,\n            Nombre: this.nuevoEmpleado.Nombre,\n            NumeroDeTelefono: this.nuevoEmpleado.NumeroDeTelefono,\n            Rol: this.nuevoEmpleado.Rol,\n            activo: this.nuevoEmpleado.activo\n          });\n          this.datosempl.push({\n            ...this.nuevoEmpleado\n          });\n        }\n        this.resetearFormulario();\n      } catch (error) {\n        console.error(\"Error al enviar los datos del empleado:\", error);\n      }\n    },\n    editarEmpleado(empleado) {\n      this.nuevoEmpleado = {\n        ...empleado\n      };\n      this.modoEdicion = true;\n      this.mostrarFormulario = true;\n    },\n    async borrarEmpleado(IDEmpleado) {\n      if (confirm('¿Está seguro de que desea eliminar este empleado?')) {\n        try {\n          this.datosempl = this.datosempl.filter(emp => emp.IDEmpleado !== IDEmpleado);\n          await axios.delete(`http://localhost:3000/Neg/empleados/${IDEmpleado}`);\n        } catch (error) {\n          console.error(\"Error al eliminar el empleado:\", error);\n        }\n      }\n    },\n    resetearFormulario() {\n      this.nuevoEmpleado = {\n        IDEmpleado: null,\n        Nombre: '',\n        NumeroDeTelefono: '',\n        Rol: '',\n        activo: true\n      };\n      this.mostrarFormulario = false;\n      this.modoEdicion = false;\n    },\n    cancelarEdicion() {\n      this.resetearFormulario();\n    },\n    async obtenerEmpleados() {\n      try {\n        const response = await axios.get('http://localhost:3000/Neg/empleadosget');\n        this.datosempl = response.data.results;\n      } catch (error) {\n        console.error('Error al obtener los empleados:', error);\n        alert('Error al obtener los empleados');\n      }\n    },\n    validarTelefono(event) {\n      const input = event.target;\n      if (!/^\\d*$/.test(input.value)) {\n        input.value = input.value.replace(/[^\\d]/g, '');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","mostrarFormulario","datosempl","nuevoEmpleado","IDEmpleado","Nombre","NumeroDeTelefono","Rol","activo","modoEdicion","opcionesCargo","mounted","obtenerEmpleados","methods","agregarEmpleado","put","index","findIndex","emp","alert","Date","now","post","push","resetearFormulario","error","console","editarEmpleado","empleado","borrarEmpleado","confirm","filter","delete","cancelarEdicion","response","get","results","validarTelefono","event","input","target","test","value","replace"],"sources":["C:\\Users\\Kenny\\Downloads\\Proyect\\Proyect_Web\\src\\components\\Empleados.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <nav class=\"navbar bg-body-tertiary\">\r\n      <div class=\"container-fluid\">\r\n        <span class=\"navbar-brand\">\r\n          <h1>Empleados</h1>\r\n        </span>\r\n        <button v-if=\"!mostrarFormulario\" @click=\"mostrarFormulario = true\" class=\"btn btn-primary\">Añadir Empleado</button>\r\n      </div>\r\n    </nav>\r\n    <div class=\"row mt-4\">\r\n      <div class=\"col-lg-12\">\r\n        <table v-if=\"!mostrarFormulario\" class=\"table table-striped\">\r\n          <thead>\r\n            <tr>\r\n              <th scope=\"col\">Nombre del empleado</th>\r\n              <th scope=\"col\">Núm. de Teléfono</th>\r\n              <th scope=\"col\">Cargo</th>\r\n              <th scope=\"col\">Acciones</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody class=\"table-group-divider\">\r\n            <tr v-for=\"(empleado, indice) in datosempl\" :key=\"indice\">\r\n              <td class=\"d-none\">{{ empleado.IDEmpleado }}</td>\r\n              <td>{{ empleado.Nombre }}</td>\r\n              <td>{{ empleado.NumeroDeTelefono }}</td>\r\n              <td>{{ empleado.Cargo }}</td>\r\n              <td>\r\n                <div class=\"btn-group\" role=\"group\">\r\n                  <button class=\"btn btn-secondary\" title=\"Editar\" @click=\"editarEmpleado(empleado)\">\r\n                    <i class=\"bi bi-pencil-square\"></i> Editar\r\n                  </button>\r\n                  <button class=\"btn btn-danger\" title=\"Eliminar\" @click=\"borrarEmpleado(empleado.IDEmpleado)\">\r\n                    <i class=\"bi bi-trash\"></i> Eliminar\r\n                  </button>\r\n                </div>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n        <form v-if=\"mostrarFormulario\" @submit.prevent=\"agregarEmpleado\">\r\n          <table class=\"table\">\r\n            <thead>\r\n              <tr>\r\n                <th scope=\"col\">Nombre del empleado</th>\r\n                <th scope=\"col\">Núm. de Teléfono</th>\r\n                <th scope=\"col\">Rol</th>\r\n                <th scope=\"col\">Estado</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody class=\"table-group-divider\">\r\n              <tr>\r\n                <td><input v-model=\"nuevoEmpleado.Nombre\" type=\"text\" required class=\"form-control\"></td>\r\n                <td>\r\n                  <input v-model=\"nuevoEmpleado.NumeroDeTelefono\" type=\"text\" required class=\"form-control\"\r\n                    @input=\"validarTelefono\" pattern=\"[0-9]*\">\r\n                </td>\r\n                <td>\r\n                  <select v-model=\"nuevoEmpleado.Rol\" required class=\"form-select\">\r\n                    <option value=\"\">Seleccione un Rol</option>\r\n                    <option v-for=\"Rol in opcionesCargo\" :key=\"Rol\" :value=\"Rol\" selected=\"\">\r\n                      {{ Rol}}\r\n                    </option>\r\n                  </select>\r\n                </td>\r\n                <td>\r\n                  <div class=\"form-check\">\r\n                    <input v-model=\"nuevoEmpleado.activo\" class=\"form-check-input\" type=\"checkbox\" id=\"empleadoActivo\">\r\n                    <label class=\"form-check-label\" for=\"empleadoActivo\">\r\n                      Activo\r\n                    </label>\r\n                  </div>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n          <button type=\"submit\" class=\"btn btn-primary\">{{ modoEdicion ? 'Actualizar' : 'Añadir' }} Empleado</button>\r\n          <button @click=\"cancelarEdicion\" type=\"button\" class=\"btn btn-secondary ml-2\">Cancelar</button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'EmpleadosApp',\r\n  data() {\r\n    return {\r\n      mostrarFormulario: false,\r\n      datosempl: [],\r\n      nuevoEmpleado: {\r\n        IDEmpleado: null, \r\n        Nombre: '',\r\n        NumeroDeTelefono: '',\r\n        Rol: '',\r\n        activo: true\r\n      },\r\n      modoEdicion: false,\r\n      opcionesCargo: ['Encargado', 'Empleado']\r\n    }\r\n  },\r\n  mounted() {\r\n    this.obtenerEmpleados();\r\n  },\r\n  methods: {\r\n    async agregarEmpleado() {\r\n      try {\r\n        if (this.modoEdicion) {\r\n          await axios.put('http://localhost:3000/Neg/empleadosAct', {\r\n            IDEmpleado: this.nuevoEmpleado.IDEmpleado,\r\n            Nombre: this.nuevoEmpleado.Nombre,\r\n            NumeroDeTelefono: this.nuevoEmpleado.NumeroDeTelefono,\r\n            Rol: this.nuevoEmpleado.Rol,\r\n            activo: this.nuevoEmpleado.activo\r\n          });\r\n          const index = this.datosempl.findIndex(emp => emp.IDEmpleado === this.nuevoEmpleado.IDEmpleado);\r\n          if (index !== -1) {\r\n            this.datosempl[index] = { ...this.nuevoEmpleado };\r\n          }\r\n          alert(\"datos enviados\")\r\n        } else {\r\n          this.nuevoEmpleado.IDEmpleado = Date.now();\r\n          await axios.post('http://localhost:3000/Neg/empleados', {\r\n            IDEmpleado: this.nuevoEmpleado.IDEmpleado, \r\n            Nombre: this.nuevoEmpleado.Nombre,\r\n            NumeroDeTelefono: this.nuevoEmpleado.NumeroDeTelefono,\r\n            Rol: this.nuevoEmpleado.Rol,\r\n            activo: this.nuevoEmpleado.activo\r\n          });\r\n          this.datosempl.push({ ...this.nuevoEmpleado });\r\n        }\r\n        this.resetearFormulario();\r\n      } catch (error) {\r\n        console.error(\"Error al enviar los datos del empleado:\", error);\r\n      }\r\n    },\r\n    editarEmpleado(empleado) {\r\n      this.nuevoEmpleado = { ...empleado };\r\n      this.modoEdicion = true;\r\n      this.mostrarFormulario = true;\r\n    },\r\n    async borrarEmpleado(IDEmpleado) {\r\n      if (confirm('¿Está seguro de que desea eliminar este empleado?')) {\r\n        try {\r\n          this.datosempl = this.datosempl.filter(emp => emp.IDEmpleado !== IDEmpleado);\r\n          await axios.delete(`http://localhost:3000/Neg/empleados/${IDEmpleado}`);\r\n        } catch (error) {\r\n          console.error(\"Error al eliminar el empleado:\", error);\r\n        }\r\n      }\r\n    },\r\n    resetearFormulario() {\r\n      this.nuevoEmpleado = {\r\n        IDEmpleado: null,\r\n        Nombre: '',\r\n        NumeroDeTelefono: '',\r\n        Rol: '',\r\n        activo: true\r\n      };\r\n      this.mostrarFormulario = false;\r\n      this.modoEdicion = false;\r\n    },\r\n    cancelarEdicion() {\r\n      this.resetearFormulario();\r\n    },\r\n    async obtenerEmpleados() {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/Neg/empleadosget');\r\n        this.datosempl = response.data.results;\r\n      } catch (error) {\r\n        console.error('Error al obtener los empleados:', error);\r\n        alert('Error al obtener los empleados');\r\n      }\r\n    },\r\n    validarTelefono(event) {\r\n      const input = event.target;\r\n      if (!/^\\d*$/.test(input.value)) {\r\n        input.value = input.value.replace(/[^\\d]/g, '');\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.movements {\r\n  padding: 80px;\r\n  background-color: #fff;\r\n  border-radius: 5px;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n  border-bottom: 1px solid #ccc;\r\n  padding-bottom: 10px;\r\n}\r\n\r\n.employee-table {\r\n  width: 100%;\r\n  background-color: #fff;\r\n  border-radius: 5px;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n  overflow: hidden; /* Asegura que el contenido no desborde */\r\n}\r\n\r\n.table {\r\n  width: 100%;\r\n}\r\n\r\n.table th,\r\n.table td {\r\n  padding: 12px;\r\n  text-align: left;\r\n}\r\n\r\n.table-striped tbody tr:nth-of-type(odd) {\r\n  background-color: rgba(0, 0, 0, 0.05);\r\n}\r\n\r\n.form-check-input {\r\n  margin-top: 0.3rem;\r\n}\r\n\r\n.form-check-label {\r\n  margin-left: 1rem;\r\n}\r\n</style>\r\n"],"mappings":";AAqFA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,iBAAiB,EAAE,KAAK;MACxBC,SAAS,EAAE,EAAE;MACbC,aAAa,EAAE;QACbC,UAAU,EAAE,IAAI;QAChBC,MAAM,EAAE,EAAE;QACVC,gBAAgB,EAAE,EAAE;QACpBC,GAAG,EAAE,EAAE;QACPC,MAAM,EAAE;MACV,CAAC;MACDC,WAAW,EAAE,KAAK;MAClBC,aAAa,EAAE,CAAC,WAAW,EAAE,UAAU;IACzC;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,gBAAgB,CAAC,CAAC;EACzB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,IAAI,IAAI,CAACL,WAAW,EAAE;UACpB,MAAMX,KAAK,CAACiB,GAAG,CAAC,wCAAwC,EAAE;YACxDX,UAAU,EAAE,IAAI,CAACD,aAAa,CAACC,UAAU;YACzCC,MAAM,EAAE,IAAI,CAACF,aAAa,CAACE,MAAM;YACjCC,gBAAgB,EAAE,IAAI,CAACH,aAAa,CAACG,gBAAgB;YACrDC,GAAG,EAAE,IAAI,CAACJ,aAAa,CAACI,GAAG;YAC3BC,MAAM,EAAE,IAAI,CAACL,aAAa,CAACK;UAC7B,CAAC,CAAC;UACF,MAAMQ,KAAI,GAAI,IAAI,CAACd,SAAS,CAACe,SAAS,CAACC,GAAE,IAAKA,GAAG,CAACd,UAAS,KAAM,IAAI,CAACD,aAAa,CAACC,UAAU,CAAC;UAC/F,IAAIY,KAAI,KAAM,CAAC,CAAC,EAAE;YAChB,IAAI,CAACd,SAAS,CAACc,KAAK,IAAI;cAAE,GAAG,IAAI,CAACb;YAAc,CAAC;UACnD;UACAgB,KAAK,CAAC,gBAAgB;QACxB,OAAO;UACL,IAAI,CAAChB,aAAa,CAACC,UAAS,GAAIgB,IAAI,CAACC,GAAG,CAAC,CAAC;UAC1C,MAAMvB,KAAK,CAACwB,IAAI,CAAC,qCAAqC,EAAE;YACtDlB,UAAU,EAAE,IAAI,CAACD,aAAa,CAACC,UAAU;YACzCC,MAAM,EAAE,IAAI,CAACF,aAAa,CAACE,MAAM;YACjCC,gBAAgB,EAAE,IAAI,CAACH,aAAa,CAACG,gBAAgB;YACrDC,GAAG,EAAE,IAAI,CAACJ,aAAa,CAACI,GAAG;YAC3BC,MAAM,EAAE,IAAI,CAACL,aAAa,CAACK;UAC7B,CAAC,CAAC;UACF,IAAI,CAACN,SAAS,CAACqB,IAAI,CAAC;YAAE,GAAG,IAAI,CAACpB;UAAc,CAAC,CAAC;QAChD;QACA,IAAI,CAACqB,kBAAkB,CAAC,CAAC;MAC3B,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,yCAAyC,EAAEA,KAAK,CAAC;MACjE;IACF,CAAC;IACDE,cAAcA,CAACC,QAAQ,EAAE;MACvB,IAAI,CAACzB,aAAY,GAAI;QAAE,GAAGyB;MAAS,CAAC;MACpC,IAAI,CAACnB,WAAU,GAAI,IAAI;MACvB,IAAI,CAACR,iBAAgB,GAAI,IAAI;IAC/B,CAAC;IACD,MA