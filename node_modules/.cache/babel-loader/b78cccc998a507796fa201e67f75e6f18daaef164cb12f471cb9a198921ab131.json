{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'ParteLateral',\n  data() {\n    return {\n      isOpen: false,\n      isMdAndUp: window.innerWidth >= 768,\n      negocios: [],\n      selectedNegocio: null\n    };\n  },\n  mounted() {\n    window.addEventListener('resize', this.handleResize);\n    this.handleResize();\n    this.cargarNegocios();\n  },\n  beforeUnmount() {\n    window.removeEventListener('resize', this.handleResize);\n  },\n  methods: {\n    toggleSidebar() {\n      this.isOpen = !this.isOpen;\n    },\n    closeSidebar() {\n      this.isOpen = false;\n    },\n    handleResize() {\n      this.isMdAndUp = window.innerWidth >= 768;\n    },\n    async cargarNegocios() {\n      try {\n        console.log('Cargando negocios...');\n        const response = await axios.get('http://localhost:3000/Neg/getnegocios');\n        console.log('Respuesta del servidor:', response.data);\n        this.$nextTick(() => {\n          this.negocios = response.data;\n          console.log('Negocios cargados:', this.negocios);\n          if (this.negocios.length > 0 && !this.selectedNegocio) {\n            this.selectedNegocio = this.negocios[0].id;\n            this.cambiarNegocio();\n          }\n        });\n      } catch (error) {\n        console.error('Error al cargar negocios:', error);\n      }\n    },\n    async cambiarNegocio() {\n      if (!this.selectedNegocio) return;\n      console.log('Negocio seleccionado:', this.selectedNegocio);\n      try {\n        // Cargar datos específicos del negocio seleccionado\n        const response = await axios.get(`http://localhost:3000/Neg/getNegocio/${this.selectedNegocio}`);\n        console.log('Datos del negocio seleccionado:', response.data);\n\n        // Actualizar el estado global de la aplicación\n        // Asumiendo que estás usando Vuex para el manejo del estado global\n        this.$store.commit('setNegocioActual', response.data);\n\n        // Emitir un evento para notificar a los componentes padres\n        this.$emit('negocio-cambiado', response.data);\n\n        // Redirigir a la página principal del negocio o actualizar la vista actual\n        // Esto dependerá de cómo esté estructurada tu aplicación\n        this.$router.push({\n          name: 'DashboardNegocio',\n          params: {\n            id: this.selectedNegocio\n          }\n        });\n      } catch (error) {\n        console.error('Error al cargar los datos del negocio:', error);\n        // Manejar el error (mostrar una notificación al usuario, etc.)\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","isOpen","isMdAndUp","window","innerWidth","negocios","selectedNegocio","mounted","addEventListener","handleResize","cargarNegocios","beforeUnmount","removeEventListener","methods","toggleSidebar","closeSidebar","console","log","response","get","$nextTick","length","id","cambiarNegocio","error","$store","commit","$emit","$router","push","params"],"sources":["C:\\Users\\R\\Documents\\Tareas 9cuatri\\Proyecto\\Proyect_Web\\src\\components\\ParteLateral.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <button @click=\"toggleSidebar\" class=\"btn btn-primary sidebar-toggle d-md-none\">\r\n      <i :class=\"['bi', isOpen ? 'bi-x' : 'bi-list']\"></i>\r\n    </button>\r\n\r\n    <nav :class=\"['sidebar', isOpen || isMdAndUp ? 'sidebar-open' : '', 'd-md-block']\">\r\n      <div class=\"container-fluid\">\r\n        <button @click=\"closeSidebar\" class=\"btn btn-close d-md-none\" aria-label=\"Close\">\r\n          <i class=\"bi bi-x\"></i>\r\n        </button>\r\n        <div class=\"col-12 perfiles\">\r\n          <select v-model=\"selectedNegocio\" class=\"form-select\" @change=\"cambiarNegocio\">\r\n            <option value=\"\" disabled>Selecciona un negocio</option>\r\n            <option v-for=\"negocio in negocios\" :key=\"negocio.id\" :value=\"negocio.id\">\r\n              {{ negocio.nameneg || negocio.nombre }}\r\n            </option>\r\n          </select>\r\n        </div>\r\n        <div class=\"config\">\r\n          <div class=\"config-item\">\r\n            <i class=\"bi bi-gear-fill\"></i>\r\n            <RouterLink to=\"/InfoNeg\" @click=\"closeSidebar\">Configuraciones</RouterLink>\r\n          </div>\r\n          <div class=\"config-item\">\r\n            <i class=\"bi bi-plus-circle-fill\"></i>\r\n            <RouterLink to=\"/AgregarNegocio\" @click=\"closeSidebar\">Agregar negocio</RouterLink>\r\n          </div>\r\n        </div>\r\n        <div class=\"produc\">\r\n          <div class=\"produc-item\">\r\n            <i class=\"bi bi-file-earmark-minus-fill\"></i>\r\n            <RouterLink to=\"/Movimientos\" @click=\"closeSidebar\">Movimientos</RouterLink>\r\n          </div>\r\n          <div class=\"produc-item\">\r\n            <i class=\"bi bi-box-seam-fill\"></i>\r\n            <RouterLink to=\"/Productos\" @click=\"closeSidebar\">Productos</RouterLink>\r\n          </div>\r\n          <div class=\"produc-item\">\r\n            <i class=\"bi bi-people-fill\"></i>\r\n            <RouterLink to=\"/Empleados\" @click=\"closeSidebar\">Empleados</RouterLink>\r\n          </div>\r\n        </div>\r\n        <div class=\"gest\">\r\n          <h5>Gestiona tus contactos</h5>\r\n          <RouterLink to=\"/ClientesTreinta\" @click=\"closeSidebar\">Clientes</RouterLink>\r\n          <br>\r\n          <RouterLink to=\"/Proveedores\" @click=\"closeSidebar\">Proveedores</RouterLink>\r\n        </div>\r\n      </div>\r\n    </nav>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'ParteLateral',\r\n  data() {\r\n    return {\r\n      isOpen: false,\r\n      isMdAndUp: window.innerWidth >= 768,\r\n      negocios: [],\r\n      selectedNegocio: null\r\n    }\r\n  },\r\n  mounted() {\r\n    window.addEventListener('resize', this.handleResize);\r\n    this.handleResize();\r\n    this.cargarNegocios();\r\n  },\r\n  beforeUnmount() {\r\n    window.removeEventListener('resize', this.handleResize);\r\n  },\r\n  methods: {\r\n    toggleSidebar() {\r\n      this.isOpen = !this.isOpen;\r\n    },\r\n    closeSidebar() {\r\n      this.isOpen = false;\r\n    },\r\n    handleResize() {\r\n      this.isMdAndUp = window.innerWidth >= 768;\r\n    },\r\n    async cargarNegocios() {\r\n      try {\r\n        console.log('Cargando negocios...');\r\n        const response = await axios.get('http://localhost:3000/Neg/getnegocios');\r\n        console.log('Respuesta del servidor:', response.data);\r\n        this.$nextTick(() => {\r\n          this.negocios = response.data;\r\n          console.log('Negocios cargados:', this.negocios);\r\n          if (this.negocios.length > 0 && !this.selectedNegocio) {\r\n            this.selectedNegocio = this.negocios[0].id;\r\n            this.cambiarNegocio();\r\n          }\r\n        });\r\n      } catch (error) {\r\n        console.error('Error al cargar negocios:', error);\r\n      }\r\n    },\r\n    async cambiarNegocio() {\r\n      if (!this.selectedNegocio) return;\r\n\r\n      console.log('Negocio seleccionado:', this.selectedNegocio);\r\n      \r\n      try {\r\n        // Cargar datos específicos del negocio seleccionado\r\n        const response = await axios.get(`http://localhost:3000/Neg/getNegocio/${this.selectedNegocio}`);\r\n        console.log('Datos del negocio seleccionado:', response.data);\r\n\r\n        // Actualizar el estado global de la aplicación\r\n        // Asumiendo que estás usando Vuex para el manejo del estado global\r\n        this.$store.commit('setNegocioActual', response.data);\r\n\r\n        // Emitir un evento para notificar a los componentes padres\r\n        this.$emit('negocio-cambiado', response.data);\r\n\r\n        // Redirigir a la página principal del negocio o actualizar la vista actual\r\n        // Esto dependerá de cómo esté estructurada tu aplicación\r\n        this.$router.push({ name: 'DashboardNegocio', params: { id: this.selectedNegocio }});\r\n\r\n      } catch (error) {\r\n        console.error('Error al cargar los datos del negocio:', error);\r\n        // Manejar el error (mostrar una notificación al usuario, etc.)\r\n      }\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.sidebar-toggle {\r\n  margin-bottom: 20px;\r\n  display: flex;\r\n  justify-content: flex-start;\r\n  align-items: center;\r\n  gap: 10px;\r\n}\r\n\r\n.sidebar {\r\n  background-color: #f8f9fa;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n  transition: transform 0.3s ease-in-out, opacity 0.3s ease-in-out;\r\n  position: fixed;\r\n  top: 0;\r\n  bottom: 0;\r\n  left: 0;\r\n  z-index: 1000;\r\n  overflow-y: auto;\r\n  padding: 20px;\r\n  border-right: 1px solid #ddd;\r\n  width: 250px;\r\n  transform: translateX(-100%);\r\n  opacity: 0;\r\n}\r\n\r\n.sidebar-open {\r\n  transform: translateX(0);\r\n  opacity: 1;\r\n}\r\n\r\n.container-fluid {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 20px;\r\n}\r\n\r\n.config-item,\r\n.produc-item {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 10px;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.config-item:hover,\r\n.produc-item:hover {\r\n  background-color: #e9ecef;\r\n}\r\n\r\n.gest h5 {\r\n  margin-bottom: 10px;\r\n  font-weight: bold;\r\n  color: #495057;\r\n}\r\n\r\n.gest a {\r\n  display: block;\r\n  margin-bottom: 5px;\r\n  text-decoration: none;\r\n  color: #007bff;\r\n}\r\n\r\n.gest a:hover {\r\n  text-decoration: underline;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .sidebar {\r\n    transform: none;\r\n    opacity: 1;\r\n    position: static;\r\n    border-right: none;\r\n    width: 100%;\r\n  }\r\n}\r\n</style>"],"mappings":";AAuDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,KAAK;MACbC,SAAS,EAAEC,MAAM,CAACC,UAAS,IAAK,GAAG;MACnCC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE;IACnB;EACF,CAAC;EAC