{"ast":null,"code":"import axios from 'axios';\nimport Swal from 'sweetalert2';\nexport default {\n  name: 'CerrarCaja',\n  data() {\n    return {\n      datosCajaAbierta: null,\n      montoFinal: '',\n      fechaHoraCierre: ''\n    };\n  },\n  computed: {\n    balanceTotal() {\n      if (this.datosCajaAbierta) {\n        return this.datosCajaAbierta.totalVentas - this.datosCajaAbierta.totalGastos;\n      }\n      return 0;\n    },\n    montoMaximo() {\n      if (this.datosCajaAbierta) {\n        return this.balanceTotal + parseFloat(this.datosCajaAbierta.MontoInicial);\n      }\n      return 0;\n    }\n  },\n  async created() {\n    try {\n      const response = await axios.get('http://localhost:3000/CajaAbierta');\n      this.datosCajaAbierta = response.data;\n    } catch (error) {\n      console.error('Error al obtener datos de la caja:', error);\n      Swal.fire({\n        icon: 'error',\n        title: 'Error',\n        text: 'No se pudieron obtener los datos de la caja.'\n      });\n    }\n    this.fechaHoraCierre = new Date().toISOString().slice(0, 16);\n  },\n  methods: {\n    validarMonto() {\n      if (this.montoFinal < 0) {\n        this.montoFinal = 0;\n      } else if (this.montoFinal > this.montoMaximo) {\n        this.montoFinal = this.montoMaximo;\n      }\n    },\n    async MontoFinalCaja() {\n      const formData = {\n        montoFinal: this.montoFinal,\n        fechaHoraCierre: this.fechaHoraCierre,\n        IDAbrirCaja: this.datosCajaAbierta.IDAbrirCaja\n      };\n      try {\n        const response = await axios.post('http://localhost:3000/cierrecaja', formData);\n        console.log('Respuesta del servidor:', response.data);\n        await Swal.fire({\n          icon: 'success',\n          title: 'Éxito',\n          text: 'Se cerró caja correctamente',\n          timer: 2000,\n          showConfirmButton: false\n        });\n        this.limpiarDatos();\n        this.$emit('caja-cerrada');\n      } catch (error) {\n        console.error('Error al cerrar caja:', error);\n        Swal.fire({\n          icon: 'error',\n          title: 'Error',\n          text: 'Error al cerrar caja: ' + error.message\n        });\n      }\n    },\n    limpiarDatos() {\n      this.montoFinal = '';\n    }\n  }\n};","map":{"version":3,"names":["axios","Swal","name","data","datosCajaAbierta","montoFinal","fechaHoraCierre","computed","balanceTotal","totalVentas","totalGastos","montoMaximo","parseFloat","MontoInicial","created","response","get","error","console","fire","icon","title","text","Date","toISOString","slice","methods","validarMonto","MontoFinalCaja","formData","IDAbrirCaja","post","log","timer","showConfirmButton","limpiarDatos","$emit","message"],"sources":["C:\\Users\\Kenny\\Downloads\\Proyect\\Proyect_Web\\src\\components\\CerrarCaja.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <p v-if=\"datosCajaAbierta\">ID Caja: {{ datosCajaAbierta.IDAbrirCaja }}</p>\r\n    <p v-if=\"datosCajaAbierta\">Fecha y Hora: {{ datosCajaAbierta.Fecha }}</p>\r\n    <p v-if=\"datosCajaAbierta\">Monto Inicial: {{ datosCajaAbierta.MontoInicial }}</p>\r\n    <p v-if=\"datosCajaAbierta\">Ventas totales: {{ datosCajaAbierta.totalVentas }}</p>\r\n    <p v-if=\"datosCajaAbierta\">Gastos totales: {{ datosCajaAbierta.totalGastos }}</p>\r\n    <p v-if=\"datosCajaAbierta\">Balance Total: {{ balanceTotal }}</p>\r\n\r\n    <form @submit.prevent=\"MontoFinalCaja\">\r\n      <div class=\"form-group\">\r\n        <label for=\"montoFinal\">Total de dinero en efectivo:</label>\r\n        <input\r\n          type=\"number\"\r\n          id=\"montoFinal\"\r\n          v-model.number=\"montoFinal\"\r\n          class=\"form-control\"\r\n          placeholder=\"Ingrese el monto final\"\r\n          required\r\n          :min=\"0\"\r\n          :max=\"montoMaximo\"\r\n          @input=\"validarMonto\"\r\n        />\r\n      </div>\r\n\r\n      <button type=\"submit\" class=\"btn btn-primary mt-3\">Cerrar Caja</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport Swal from 'sweetalert2';\r\n\r\nexport default {\r\n  name: 'CerrarCaja',\r\n  data() {\r\n    return {\r\n      datosCajaAbierta: null,\r\n      montoFinal: '',\r\n      fechaHoraCierre: '',\r\n    };\r\n  },\r\n  computed: {\r\n    balanceTotal() {\r\n      if (this.datosCajaAbierta) {\r\n        return this.datosCajaAbierta.totalVentas - this.datosCajaAbierta.totalGastos;\r\n      }\r\n      return 0;\r\n    },\r\n    montoMaximo() {\r\n      if (this.datosCajaAbierta) {\r\n        return this.balanceTotal + parseFloat(this.datosCajaAbierta.MontoInicial);\r\n      }\r\n      return 0;\r\n    },\r\n  },\r\n  async created() {\r\n    try {\r\n      const response = await axios.get('http://localhost:3000/CajaAbierta');\r\n      this.datosCajaAbierta = response.data;\r\n    } catch (error) {\r\n      console.error('Error al obtener datos de la caja:', error);\r\n      Swal.fire({\r\n        icon: 'error',\r\n        title: 'Error',\r\n        text: 'No se pudieron obtener los datos de la caja.',\r\n      });\r\n    }\r\n    this.fechaHoraCierre = new Date().toISOString().slice(0, 16);\r\n  },\r\n  methods: {\r\n    validarMonto() {\r\n      if (this.montoFinal < 0) {\r\n        this.montoFinal = 0;\r\n      } else if (this.montoFinal > this.montoMaximo) {\r\n        this.montoFinal = this.montoMaximo;\r\n      }\r\n    },\r\n    async MontoFinalCaja() {\r\n      const formData = {\r\n        montoFinal: this.montoFinal,\r\n        fechaHoraCierre: this.fechaHoraCierre,\r\n        IDAbrirCaja: this.datosCajaAbierta.IDAbrirCaja,\r\n      };\r\n      try {\r\n        const response = await axios.post('http://localhost:3000/cierrecaja', formData);\r\n        console.log('Respuesta del servidor:', response.data);\r\n        \r\n        await Swal.fire({\r\n          icon: 'success',\r\n          title: 'Éxito',\r\n          text: 'Se cerró caja correctamente',\r\n          timer: 2000,\r\n          showConfirmButton: false\r\n        });\r\n\r\n        this.limpiarDatos();\r\n        this.$emit('caja-cerrada');\r\n      } catch (error) {\r\n        console.error('Error al cerrar caja:', error);\r\n        Swal.fire({\r\n          icon: 'error',\r\n          title: 'Error',\r\n          text: 'Error al cerrar caja: ' + error.message,\r\n        });\r\n      }\r\n    },\r\n    limpiarDatos() {\r\n      this.montoFinal = '';\r\n    },\r\n  },\r\n};\r\n</script>\r\n  \r\n  <style scoped>\r\n  .form-group {\r\n    margin-bottom: 15px;\r\n  }\r\n  \r\n  .btn-primary {\r\n    background-color: #007bff;\r\n    border-color: #007bff;\r\n    text-align: end;\r\n  }\r\n  \r\n  .btn-primary:hover {\r\n    background-color: #0056b3;\r\n    border-color: #0056b3;\r\n  }\r\n  </style>\r\n  "],"mappings":"AA+BA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAE9B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,gBAAgB,EAAE,IAAI;MACtBC,UAAU,EAAE,EAAE;MACdC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,YAAYA,CAAA,EAAG;MACb,IAAI,IAAI,CAACJ,gBAAgB,EAAE;QACzB,OAAO,IAAI,CAACA,gBAAgB,CAACK,WAAU,GAAI,IAAI,CAACL,gBAAgB,CAACM,WAAW;MAC9E;MACA,OAAO,CAAC;IACV,CAAC;IACDC,WAAWA,CAAA,EAAG;MACZ,IAAI,IAAI,CAACP,gBAAgB,EAAE;QACzB,OAAO,IAAI,CAACI,YAAW,GAAII,UAAU,CAAC,IAAI,CAACR,gBAAgB,CAACS,YAAY,CAAC;MAC3E;MACA,OAAO,CAAC;IACV;EACF,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMC,QAAO,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAAC,mCAAmC,CAAC;MACrE,IAAI,CAACZ,gBAAe,GAAIW,QAAQ,CAACZ,IAAI;IACvC,EAAE,OAAOc,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,oCAAoC,EAAEA,KAAK,CAAC;MAC1DhB,IAAI,CAACkB,IAAI,CAAC;QACRC,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE,OAAO;QACdC,IAAI,EAAE;MACR,CAAC,CAAC;IACJ;IACA,IAAI,CAAChB,eAAc,GAAI,IAAIiB,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,CAAC,EAAE,EAAE,CAAC;EAC9D,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,IAAI,IAAI,CAACtB,UAAS,GAAI,CAAC,EAAE;QACvB,IAAI,CAACA,UAAS,GAAI,CAAC;MACrB,OAAO,IAAI,IAAI,CAACA,UAAS,GAAI,IAAI,CAACM,WAAW,EAAE;QAC7C,IAAI,CAACN,UAAS,GAAI,IAAI,CAACM,WAAW;M