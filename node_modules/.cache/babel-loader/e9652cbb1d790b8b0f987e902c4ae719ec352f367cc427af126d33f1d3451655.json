{"ast":null,"code":"import axios from 'axios';\nexport default {\n  props: {\n    isVisible: {\n      type: Boolean,\n      required: true\n    },\n    fechaHora: {\n      type: String,\n      default: ''\n    },\n    montoInicial: {\n      type: Number,\n      default: 0\n    }\n  },\n  data() {\n    return {\n      tab: 'efectivo',\n      efectivo: 0\n    };\n  },\n  computed: {\n    descuadre() {\n      return this.montoInicial - this.efectivo;\n    }\n  },\n  methods: {\n    closeModal() {\n      this.$emit('close');\n    },\n    confirmarCierre() {\n      this.closeModal();\n    },\n    fetchData() {\n      axios.get('http://localhost:3000/AbrirCaja').then(response => {\n        this.montoInicial = response.data.montoInicial;\n        this.fechaHora = response.data.fechaHora;\n      }).catch(error => {\n        console.error('Error al obtener datos de caja:', error);\n      });\n    }\n  },\n  watch: {\n    isVisible(newVal) {\n      if (newVal) {\n        this.fetchData();\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","props","isVisible","type","Boolean","required","fechaHora","String","default","montoInicial","Number","data","tab","efectivo","computed","descuadre","methods","closeModal","$emit","confirmarCierre","fetchData","get","then","response","catch","error","console","watch","newVal"],"sources":["D:\\Vers_Git_Proyecto\\Proyect_Web\\src\\components\\CierreCaja.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"isVisible\" class=\"modal\">\r\n    <div class=\"modal-content\">\r\n      <span class=\"close\" @click=\"closeModal\">&times;</span>\r\n      <h2>Cerrar Caja</h2>\r\n      <div>\r\n        <h3>Registros realizados</h3>\r\n        <div>\r\n          <button :class=\"{'active': tab === 'efectivo'}\" @click=\"tab = 'efectivo'\">Efectivo</button>\r\n        </div>\r\n        <div v-if=\"tab === 'efectivo'\">\r\n          <h4>Resumen de caja</h4>\r\n          <p>Fecha y Hora: {{ fechaHora }}</p>\r\n          <p>Monto Inicial: $ {{ montoInicial }}</p>\r\n        </div>\r\n      </div>\r\n      <div>\r\n        <label for=\"cash\">Cu√°nto dinero tienes en efectivo?</label>\r\n        <input type=\"number\" v-model=\"efectivo\" />\r\n      </div>\r\n      <div v-if=\"descuadre !== 0\" class=\"alert\">\r\n        Tienes un descuadre. Te faltan $ {{ descuadre }}\r\n      </div>\r\n      <button @click=\"confirmarCierre\">Confirmar cierre</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  props: {\r\n    isVisible: {\r\n      type: Boolean,\r\n      required: true\r\n    },\r\n    fechaHora: {\r\n      type: String,\r\n      default: ''\r\n    },\r\n    montoInicial: {\r\n      type: Number,\r\n      default: 0\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      tab: 'efectivo',\r\n      efectivo: 0\r\n    };\r\n  },\r\n  computed: {\r\n    descuadre() {\r\n      return this.montoInicial - this.efectivo;\r\n    }\r\n  },\r\n  methods: {\r\n    closeModal() {\r\n      this.$emit('close');\r\n    },\r\n    confirmarCierre() {\r\n      this.closeModal();\r\n    },\r\n    fetchData() {\r\n      axios.get('http://localhost:3000/AbrirCaja')\r\n        .then(response => {\r\n          this.montoInicial = response.data.montoInicial;\r\n          this.fechaHora = response.data.fechaHora;\r\n        })\r\n        .catch(error => {\r\n          console.error('Error al obtener datos de caja:', error);\r\n        });\r\n    }\r\n  },\r\n  watch: {\r\n    isVisible(newVal) {\r\n      if (newVal) {\r\n        this.fetchData();\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.modal {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: rgba(0, 0, 0, 0.5);\r\n}\r\n.modal-content {\r\n  background: white;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  width: 300px;\r\n}\r\n.close {\r\n  positi