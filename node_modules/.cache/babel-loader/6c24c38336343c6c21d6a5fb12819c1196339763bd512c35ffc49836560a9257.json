tons\">\r\n        <button @click=\"showSection('transacciones')\">Transacciones</button>\r\n        <button @click=\"showSection('cierresCaja')\">Cierres de caja</button>\r\n      </div>\r\n\r\n      <div class=\"transaction-section\" v-if=\"currentSection === 'transacciones'\">\r\n        <div>\r\n          <div class=\"filters\">\r\n            <select>\r\n              <option>Diario</option>\r\n              <option>Semanal</option>\r\n              <option>Mensual</option>\r\n            </select>\r\n            <input type=\"date\" />\r\n            <input type=\"text\" placeholder=\"Buscar concepto...\" />\r\n            <button>Buscar</button>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"balances\">\r\n  <span class=\"d-inline-block\" tabindex=\"0\" data-bs-toggle=\"tooltip\" title=\"Disabled tooltip\">\r\n    <button>\r\n      <h3>Balance</h3>\r\n      <p>{{ balanceTotal }}</p>\r\n    </button>\r\n    <span class=\"tooltip\">Descripción del balance</span>\r\n  </span>\r\n\r\n  <div class=\"sales\">\r\n    <h3>Ventas Totales</h3>\r\n    <p>{{ ventasTotales }}</p>\r\n    <span class=\"tooltip\">Descripción de las ventas totales</span>\r\n  </div>\r\n  \r\n  <div class=\"expenses\">\r\n    <h3>Gastos Totales</h3>\r\n    <p>{{ gastosTotales }}</p>\r\n    <span class=\"tooltip\">Descripción de los gastos totales</span>\r\n  </div>\r\n</div>\r\n\r\n\r\n\r\n        <div class=\"row\">\r\n          <div class=\"col-12\">\r\n            <ul class=\"nav nav-tabs\" id=\"myTab\" role=\"tablist\">\r\n              <li class=\"nav-item col-3\" role=\"presentation\">\r\n                <button class=\"nav-link active\" id=\"section1-tab\" data-bs-toggle=\"tab\" data-bs-target=\"#section1\" type=\"button\" role=\"tab\" aria-controls=\"section1\" aria-selected=\"true\">Ingresos</button>\r\n              </li>\r\n              <li class=\"nav-item col-3\" role=\"presentation\">\r\n                <button class=\"nav-link\" id=\"section2-tab\" data-bs-to