{"ast":null,"code":"export default {\n  name: 'CardProduct',\n  props: {\n    title: String,\n    Cantidadprod: Number,\n    descrip: String,\n    presioventa: Number,\n    presioprod: Number,\n    idprod: Number,\n    imagen: String,\n    factu: String,\n    cantidtipo: String,\n    categoriatipo: String,\n    Codigobarras: String\n  },\n  emits: [\"elimin\", \"actu\"],\n  computed: {\n    imageUrl() {\n      return `data:image/jpeg;base64,${this.imagen}`;\n    }\n  },\n  methods: {\n    async usarprop() {\n      this.$emit(\"elimin\", this.idprod);\n    },\n    async usarpropdos() {\n      this.$emit(\"actu\", {\n        id: this.idprod,\n        Categoria: this.categoriatipo,\n        CantidadVenta: this.cantidtipo,\n        Facturas: this.factu,\n        nombre: this.title,\n        cantid: this.Cantidadprod,\n        Desc: this.descrip,\n        Presiovent: this.presioventa,\n        presioprod: this.presioprod,\n        imagen: this.imagen\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","props","title","String","Cantidadprod","Number","descrip","presioventa","presioprod","idprod","imagen","factu","cantidtipo","categoriatipo","Codigobarras","emits","computed","imageUrl","methods","usarprop","$emit","usarpropdos","id","Categoria","CantidadVenta","Facturas","nombre","cantid","Desc","Presiovent"],"sources":["C:\\Users\\Kenny\\Downloads\\Proyect\\Proyect_Web\\src\\components\\CardProduct.vue"],"sourcesContent":["<template>\r\n  <div class=\"card\" style=\"width: 18rem;\">\r\n    <div class=\"card-body\">\r\n      <div class=\"post-it agotado\" v-if=\"Cantidadprod === 0\">Agotado</div>\r\n      <div class=\"post-it bajo-stock\" v-else-if=\"Cantidadprod < 10\">Bajo stock</div>\r\n      <img :src=\"imageUrl\" class=\"card-img-top\" alt=\"Product Image\" height=\"200px\" width=\"100px\" style=\"border-radius: 10px;\">\r\n      \r\n      <p class=\"card-text d-none\">{{ idprod }}</p>\r\n      <h5 class=\"card-title\">{{ title }}</h5>\r\n      <p class=\"card-text\">Precio: ${{ presioventa }}</p>\r\n      <p class=\"card-text\">Cantidad: {{ Cantidadprod }}</p>\r\n      <p class=\"card-text\">Descripción: {{ descrip }}</p>\r\n      <p class=\"card-text\">Categoría: {{ categoriatipo }}</p>\r\n      <p class=\"card-text\">Cantidad: {{ cantidtipo }}</p>\r\n      \r\n      <div>\r\n          <button class=\"btn-card-elim\" @click=\"usarprop\">Eliminar</button>\r\n          <button type=\"button\" class=\"btn-card \" data-bs-toggle=\"modal\" data-bs-target=\"#actualizarproducto\" @click=\"usarpropdos\">Actualizar Producto</button> \r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'CardProduct',\r\n  props: {\r\n      title: String,\r\n      Cantidadprod: Number,\r\n      descrip: String,\r\n      presioventa: Number,\r\n      presioprod: Number,\r\n      idprod: Number,\r\n      imagen: String,\r\n      factu: String,\r\n      cantidtipo: String,\r\n      categoriatipo:String,\r\n      Codigobarras:String,\r\n  },\r\n  emits: [\"elimin\", \"actu\"],\r\n  computed: {\r\n      imageUrl() {\r\n          return `data:image/jpeg;base64,${this.imagen}`; \r\n      }\r\n  },\r\n  methods: {\r\n    async usarprop() {\r\n      this.$emit(\"elimin\", this.idprod);\r\n    },\r\n    async usarpropdos() {\r\n      this.$emit(\"actu\",{id: this.idprod, Categoria:this.categoriatipo, CantidadVenta: this.cantidtipo, Facturas : this.factu , nombre: this.title, cantid : this.Cantidadprod, Desc : this.descrip, Presiovent : this.presioventa, presioprod :  this.presioprod, imagen: this.imagen}  );\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.card-body {\r\n    text-align: center;\r\n    border: 1px solid rgba(232, 186, 207, 0.7);\r\n    border-radius: 5px;\r\n    position: relative; /* Necesario para los post-its */\r\n}\r\n\r\n.post-it {\r\n    position: absolute;\r\n    top: 10px;\r\n    left: 10px;\r\n    width: 80px;\r\n    height: 80px;\r\n    padding: 10px;\r\n    color: white;\r\n    font-weight: bold;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    border-radius: 5px;\r\n    z-index: 10; /* Para asegurarse de que se superpone a otros elementos */\r\n}\r\n\r\n.agotado {\r\n    background-color: red;\r\n}\r\n\r\n.bajo-stock {\r\n    background-color: orange;\r\n}\r\n\r\n.btn-card{\r\n    border: 0px solid ;\r\n    height: 40px;\r\n    border-radius: 5px;\r\n    margin: 5px;\r\n    background-color: #d5e4ff;\r\n}\r\n\r\n.btn-card:hover{\r\n    background-color: #85a6ff;\r\n    color: white;\r\n}\r\n\r\n.btn-card-elim{\r\n    border: 0px solid ;\r\n    height: 40px;\r\n    border-radius: 5px;\r\n    margin: 5px;\r\n    background-color: #ffdddd;\r\n}\r\n\r\n.btn-card-elim:hover{\r\n    background-color:#ff9494;\r\n    color: white;\r\n}\r\n</style>\r\n"],"mappings":"AAwBA,eAAe;EACbA,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE;IACHC,KAAK,EAAEC,MAAM;IACbC,YAAY,EAAEC,MAAM;IACpBC,OAAO,EAAEH,MAAM;IACfI,WAAW,EAAEF,MAAM;IACnBG,UAAU,EAAEH,MAAM;IAClBI,MAAM,EAAEJ,MAAM;IACdK,MAAM,EAAEP,MAAM;IACdQ,KAAK,EAAER,MAAM;IACbS,UAAU,EAAET,MAAM;IAClBU,aAAa,EAACV,MAAM;IACpBW,YAAY,EAACX;EACjB,CAAC;EACDY,KAAK,EAAE,CAAC,QAAQ,EAAE,MAAM,CAAC;EACzBC,QAAQ,EAAE;IACNC,QAAQA,CAAA,EAAG;MACP,OAAO,0BAA0B,IAAI,CAACP,MAAM,EAAE;IAClD;EACJ,CAAC;EACDQ,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACf,IAAI,CAACC,KAAK,CAAC,QAAQ,EAAE,IAAI,CAACX,MAAM,CAAC;IACnC,CAAC;IACD,MAAMY,WAAWA,CAAA,EAAG;MAClB,IAAI,CAACD,KAAK,CAAC,MAAM,EAAC;QAACE,EAAE,EAAE,IAAI,CAACb,MAAM;QAAEc,SAAS,EAAC,IAAI,CAACV,aAAa;QAAEW,aAAa,EAAE,IAAI,CAACZ,UAAU;QAAEa,QAAO,EAAI,IAAI,CAACd,KAAI;QAAIe,MAAM,EAAE,IAAI,CAACxB,KAAK;QAAEyB,MAAK,EAAI,IAAI,CAACvB,YAAY;QAAEwB,IAAG,EAAI,IAAI,CAACtB,OAAO;QAAEuB,UAAS,EAAI,IAAI,CAACtB,WAAW;QAAEC,UAAS,EAAK,IAAI,CAACA,UAAU;QAAEE,MAAM,EAAE,IAAI,CAACA;MAAM,CAAG,CAAC;IACtR;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}