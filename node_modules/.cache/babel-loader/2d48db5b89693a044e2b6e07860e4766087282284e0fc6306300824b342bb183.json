{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'AddProductForm',\n  data() {\n    return {\n      productName: '',\n      unitCost: null,\n      quantity: 1,\n      price: null,\n      category: '',\n      description: '',\n      selectedFile: null,\n      Factura: ''\n    };\n  },\n  /*\r\n  mounted: {\r\n  },*/\n  created() {\n    axios.get('http://localhost:3000/cantidadproduc').then(response => {\n      this.datos = response.data;\n    }).catch(error => {\n      console.log('Error:', error);\n    });\n  },\n  methods: {\n    async createProduct() {\n      const formData = {\n        productName: this.productName,\n        unitCost: this.unitCost,\n        quantity: this.quantity,\n        price: this.price,\n        category: this.category,\n        description: this.description,\n        Factura: this.Factura\n      };\n      try {\n        const response = await axios.post('http://localhost:3000/items', formData);\n        console.log('Respuesta del servidor:', response.data); // Usar la respuesta\n        alert('Producto creado con éxito');\n      } catch (error) {\n        console.error('Error al crear el producto:', error);\n        alert('Error al crear el producto: ' + error.message);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","productName","unitCost","quantity","price","category","description","selectedFile","Factura","created","get","then","response","datos","catch","error","console","log","methods","createProduct","formData","post","alert","message"],"sources":["C:\\Users\\Kenny\\Downloads\\Proyect\\Proyect_Web\\src\\components\\AddProductForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-5\">\r\n    <div class=\"row justify-content-center\">\r\n      <div class=\"col-lg-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <div class=\"mb-3\">\r\n              <label for=\"fileUpload\" class=\"form-label\">Selecciona una imagen para s