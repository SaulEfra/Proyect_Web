onoModal'));\r\n      if (modal) {\r\n        modal.hide();\r\n      }\r\n    };\r\n\r\n    return {\r\n      mostrarFormulario,\r\n      clientes,\r\n      nuevoCliente,\r\n      modoEdicion,\r\n      busqueda,\r\n      clienteSeleccionado,\r\n      modalTitulo,\r\n      cantidadAdeudoAbono,\r\n      esAdeudo,\r\n      clientesFiltrados,\r\n      agregarCliente,\r\n      editarCliente,\r\n      borrarCliente,\r\n      resetearFormulario,\r\n      cancelarEdicion,\r\n      validarTelefono,\r\n      buscarCliente,\r\n      mostrarModalAdeudo,\r\n      mostrarModalAbono,\r\n      procesarAdeudoAbono,\r\n      abrirModal,\r\n      cerrarModal\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":";AAqGA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAK;AAEnC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAKA,CAAA,EAAG;IACN,MAAMC,iBAAgB,GAAIJ,GAAG,CAAC,KAAK,CAAC;IACpC,MAAMK,QAAO,GAAIL,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMM,YAAW,GAAIN,GAAG,CAAC;MACvBO,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE;IACX,CAAC,CAAC;IACF,MAAMC,WAAU,GAAIV,GAAG,CAAC,KAAK,CAAC;IAC9B,MAAMW,QAAO,GAAIX,GAAG,CAAC,EAAE,CAAC;IACxB,MAAMY,mBAAkB,GAAIZ,GAAG,CAAC,IAAI,CAAC;IACrC,MAAMa,WAAU,GAAIb,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMc,mBAAkB,GAAId,GAAG,CAAC,CAAC,CAAC;IAClC,MAAMe,QAAO,GAAIf,GAAG,CAAC,IAAI,CAAC;IAE1B,MAAMgB,iBAAgB,GAAIf,QAAQ,CAAC,MAAM;MACvC,IAAIU,QAAQ,CAACM,KAAK,EAAE;QAClB,OAAOZ,QAAQ,CAACY,KAAK,CAACC,MAAM,CAACC,OAAM,IACjCA,OAAO,CAACZ,MAAM,CAACa,WAAW,CAAC,CAAC,CAACC,QAAQ,CAACV,QAAQ,CAACM,KAAK,CAACG,WAAW,CAAC,CAAC,CACpE,CAAC;MACH;MACA,OAAOf,QAAQ,CAACY,KAAK;IACvB,CAAC,CAAC;IAEF,MAAMK,cAAa,GAAIA,CAAA,KAAM;MAC3B,IAAIZ,WAAW,CAACO,KAAK,EAAE;QACrB,MAAMM,KAAI,GAAIlB,QAAQ,CAACY,KAAK,CAACO,SAAS,CAACC,GAAE,IAAKA,GAAG,CAACC,EAAC,KAAMpB,YAAY,CAACW,KAAK,CAACS,EAAE,CAAC;QAC/E,IAAIH,KAAI,KAAM,CAAC,CAAC,EAAE;UAChBlB,QAAQ,CAACY,KAAK,CAACM,KAAK,IAAI;YAAE,GAAGjB,YAAY,CAACW;UAAM,CAAC;QACnD;MACF,OAAO;QACL,MAAMU,OAAM,GAAIC,IAAI,CAACC,GAAG,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC;QACrCzB,QAAQ,CAACY,KAAK,CAACc,IAAI,CAAC;UAAE,GAAGzB,YAAY,CAACW,KAAK;UAAES,EAAE,EAAEC;QAAQ,C