{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'ClientesTreinta',\n  data() {\n    return {\n      mostrarFormulario: false,\n      clientes: [],\n      nuevoCliente: {\n        id: '',\n        Nombre: '',\n        Telefono: '',\n        Adeudos: 0\n      },\n      modoEdicion: false\n    };\n  },\n  methods: {\n    agregarCliente() {\n      if (this.modoEdicion) {\n        const index = this.clientes.findIndex(cli => cli.id === this.nuevoCliente.id);\n        if (index !== -1) {\n          this.clientes[index] = {\n            ...this.nuevoCliente\n          };\n        }\n      } else {\n        this.clientes.push({\n          ...this.nuevoCliente\n        });\n      }\n      this.resetearFormulario();\n    },\n    editarCliente(cliente) {\n      this.nuevoCliente = {\n        ...cliente\n      };\n      this.modoEdicion = true;\n      this.mostrarFormulario = true;\n    },\n    borrarCliente(id) {\n      if (confirm('¿Está seguro de que desea eliminar este cliente?')) {\n        this.clientes = this.clientes.filter(cli => cli.id !== id);\n      }\n    },\n    resetearFormulario() {\n      this.nuevoCliente = {\n        id: '',\n        Nombre: '',\n        Telefono: '',\n        Adeudos: 0\n      };\n      this.mostrarFormulario = false;\n      this.modoEdicion = false;\n    },\n    cancelarEdicion() {\n      this.resetearFormulario();\n    },\n    validarTelefono(event) {\n      this.nuevoCliente.Telefono = event.target.value.replace(/[^0-9]/g, '');\n    }\n  }\n};","map":{"version":3,"names":["name","data","mostrarFormulario","clientes","nuevoCliente","id","Nombre","Telefono","Adeudos","modoEdicion","methods","agregarCliente","index","findIndex","cli","push","resetearFormulario","editarCliente","cliente","borrarCliente","confirm","filter","cancelarEdicion","validarTelefono","event","target","value","replace"],"sources":["C:\\Users\\R\\Documents\\Tareas 9cuatri\\Proyecto\\Proyect_Web\\src\\components\\ClientesTreinta