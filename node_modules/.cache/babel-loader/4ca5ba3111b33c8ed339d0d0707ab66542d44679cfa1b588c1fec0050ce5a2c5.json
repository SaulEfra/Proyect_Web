eg);\n      formData.append('idimg', this.idparactuproduc.id);\n      try {\n        const response = await axios.put(`http://localhost:3000/actualizarproducto/${this.idparactuproduc.id}`, formData, {\n          headers: {\n            'Content-Type': 'multipart/form-data'\n          }\n        });\n        console.log('Respuesta del servidor:', response.data);\n        alert('Producto actualizado con éxito');\n        this.resetForm();\n      } catch (error) {\n        console.error('Error al actualizar el producto:', error);\n        alert('Error al actualizar el producto: ' + error.message);\n      }\n    },\n    resetForm() {\n      this.selectfile = null;\n      this.category = '';\n      this.Factura = '';\n      // No podemos modificar props directamente, ya que es una propiedad de solo lectura\n      // Para actualizar idparactuproduc, deberías emitir un evento o usar un estado global\n    }\n  }\n};","map":{"version":3,"names":["axios","name","props","idparactuproduc","Object","data","cantVen","category","Factura","datoscant","datoscat","idNeg","selectfile","mounted","Cantidades","Categorias","methods","response","get","results","error","console","alert","onFileChange","event","target","files","actualiProduct","formData","FormData","append","nombre","presioprod","Presiovent","cantid","Desc","id","put","headers","log","resetForm","message"],"sources":["D:\\Vers_Git_Proyecto\\Proyect_Web\\src\\components\\ActProducto.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"idparactuproduc\" class=\"container mt-5\">\r\n    <div class=\"row justify-content-center\">\r\n      <div class=\"col-lg-12\">\r\n        <div class=\"card\">\r\n          <div class=\"card-body\">\r\n            <div class=\"mb-3\">\r\n              <label for=\"fileUpload\" class=\"form-label\">Selecciona una imagen para subir</label>\r\n              <input type=\"file\" class=\"form-control\" id=\"fileUpload\" accept=\"image/*\" @change=\"onFileChange\">\r\n            </div>\r\n            <div class=\"row\">\r\n              <div class=\"col-lg-6\">\r\n                <div class=\"mb-3\">\r\n                  <label for=\"NameProduct\" class=\"form-label\">Nombre de producto</label>\r\n                  <input v-model=\"idparactuproduc.nombre\" id=\"NameProduct\" type=\"text\" class=\"form-control\">\r\n                </div>\r\n                <div class=\"mb-3\">\r\n                  <label for=\"categ\" class=\"form-label\">Cantidad de venta</label>\r\n                  <select v-model=\"cantVen\" id=\"categ\" class=\"form-select\" aria-label=\"Default select example\">\r\n                    <option v-for=\"cant in datoscant\" :key=\"cant.IDCantidad\" :value=\"cant.IDCantidad\">{{ cant.NombreCantidad }}</option>\r\n                  </select>\r\n                </div>\r\n                <div class=\"mb-3\">\r\n                  <label for=\"CostUni\" class=\"form-label\">Costo Unitario</label>\r\n                  <input v-model.number=\"idparactuproduc.presioprod\" id=\"CostUni\" type=\"number\" class=\"form-control\">\r\n                </div>\r\n                <div class=\"mb-3\">\r\n                  <label for=\"preci\" class=\"form-label\">Precio</label>\r\n                  <input v-model.number=\"idparactuproduc.Presiovent\" id=\"preci\" type=\"number\" class=\"form-control\">\r\n                </div>\r\n              </div>\r\n              <div class=\"col-lg-6\">\r\n                <div class=\"mb-3\">\r\n                  <label for=\"cantidad\" class=\"form-label\">Cantidad</label>\r\n                  <input v-model.number=\"idparactuproduc.cantid\" type=\"number\" id=\"cantidad\" name=\"cantidad\" min=\"1\" max=\"100\" step=\"1\" class=\"form-control\">\r\n                </div>\r\n                <div class=\"mb-3\">\r\n                  <label for=\"category\" class=\"form-label\">Categoría</label>\r\n                  <select v-model=\"category\" id=\"category\" class=\"form-select\" aria-label=\"Default select example\">\r\n                    <option v-for=\"cat in datoscat\" :key=\"cat.IDCatProd\" :value=\"cat.IDCatProd\">{{ cat.NombreCategoria }}</option>\r\n                  </select>\r\n                </div>\r\n                <div class=\"form-check\">\r\n                  <input class=\"form-check-input\" v-model=\"Factura\" type=\"radio\" name=\"exampleRadios\" id=\"exampleRadios2\" value=\"Factura\">\r\n                  <label class=\"form-check-label\" for=\"exampleRadios2\">Factura</label>\r\n                </div>\r\n                <div class=\"mb-3\">\r\n                  <label for=\"Textarea1\" class=\"form-label\">Descripción</label>\r\n                  <textarea v-model=\"idparactuproduc.Desc\" class=\"form-control\" id=\"Textarea1\" rows=\"3\"></textarea>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"text-center mt-4\">\r\n              <button @click=\"actualiProduct\" class=\"btn btn-primary\">Actualizar producto</button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'ActProd',\r\n  props: {\r\n    idparactuproduc: Object\r\n  },\r\n  data() {\r\n    return {\r\n      cantVen: this.idparactuproduc.cantVen || '', // Inicializa con valor del prop\r\n      category: this.idparactuproduc.category || '', // Inicializa con valor del prop\r\n      Factura: this.idparactuproduc.Factura || '', // Inicializa con valor del prop\r\n      datoscant: [],\r\n      datoscat: [],\r\n      idNeg: 1,\r\n      selectfile: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.Cantidades();\r\n    this.Categorias();\r\n  },\r\n  methods: {\r\n    async Cantidades() {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/cantidadproducDos');\r\n        this.datoscant = response.data.results; \r\n      } catch (error) {\r\n        console.error('Error al obtener las cantidades:', error);\r\n        alert('Error al obtener las cantidades');\r\n      }\r\n    },\r\n    async Categorias() {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/categoriaproducto');\r\n        this.datoscat = response.data.results; \r\n      } catch (error) {\r\n        console.error('Error al obtener las categorias:', error);\r\n        alert('Error al obtener las categorias');\r\n      }\r\n    },\r\n    onFileChange(event) {\r\n      this.selectfile = event.target.files[0];\r\n    },\r\n    async actualiProduct() {\r\n      const formData = new FormData();\r\n      if (this.selectfile) {\r\n        formData.append('Datos', this.selectfile);\r\n      }\r\n      formData.append('productName', this.idparactuproduc.nombre);\r\n      formData.append('Costunit', this.idparactuproduc.presioprod);\r\n      formData.append('price', this.idparactuproduc.Presiovent);\r\n      formData.append('cantid', this.idparactuproduc.cantid);\r\n      formData.append('category', this.category);\r\n      formData.append('Factura', this.Factura);\r\n      formData.append('description', this.idparactuproduc.Desc);\r\n      formData.append('idneg', this.idNeg);\r\n      formData.append('idimg', this.idparactuproduc.id);\r\n\r\n      try {\r\n        const response = await axios.put(`http://localhost:3000/actualizarproducto/${this.idparactuproduc.id}`, formData, {\r\n          headers: {\r\n            'Content-Type': 'multipart/form-data'\r\n          }\r\n        });\r\n        console.log('Respuesta del servidor:', response.data); \r\n        alert('Producto actualizado con éxito');\r\n        this.resetForm();\r\n      } catch (error) {\r\n        console.error('Error al actualizar el producto:', error);\r\n        alert('Error al actualizar el producto: ' + error.message);\r\n      }\r\n    },\r\n    resetForm() {\r\n      this.selectfile = null;\r\n      this.category = '';\r\n      this.Factura = '';\r\n      // No podemos modificar props directamente, ya que es una propiedad de solo lectura\r\n      // Para actualizar idparactuproduc, deberías emitir un evento o usar un estado global\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  \r\n</style>\r\n"],"mappings":"AA+DA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,KAAK,EAAE;IACLC,eAAe,EAAEC;EACnB,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI,CAACH,eAAe,CAACG,OAAM,IAAK,EAAE;MAAE;MAC7CC,QAAQ,EAAE,IAAI,CAACJ,eAAe,CAACI,QAAO,IAAK,EAAE;MAAE;MAC/CC,OAAO,EAAE,IAAI,CAACL,eAAe,CAACK,OAAM,IAAK,EAAE;MAAE;MAC7CC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,CAAC;MACRC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;IACjB,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMF,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMG,QAAO,GAAI,MAAMjB,KAAK,CAACkB,GAAG,CAAC,yCAAyC,CAAC;QAC3E,IAAI,CAACT,SAAQ,GAAIQ,QAAQ,CAACZ,IAAI,CAACc,OAAO;MACxC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,kCAAkC,EAAEA,KAAK,CAAC;QACxDE,KAAK,CAAC,iCAAiC,CAAC;MAC1C;IACF,CAAC;IACD,MAAMP,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMjB,KAAK,CAACkB,GAAG,CAAC,yCAAyC,CAAC;QAC3E,IAAI,CAACR,QAAO,GAAIO,QAAQ,CAACZ,IAAI,CAACc,OAAO;MACvC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,kCAAkC,EAAEA,KAAK,CAAC;QACxDE,KAAK,CAAC,iCAAiC,CAAC;MAC1C;IACF,CAAC;IACDC,YAAYA,CAACC,KAAK,EAAE;MAClB,IAAI,CAACZ,UAAS,GAAIY,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IACzC,CAAC;IACD,MAAMC,cAAcA,CAAA,EAAG;MACrB,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/B,IAAI,IAAI,CAACjB,UAAU,EAAE;QACnBgB,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAAClB,UAAU,CAAC;MAC3C;MACAgB,QAAQ,CAACE,MAAM,CAAC,aAAa,EAAE,IAAI,CAAC3B,eAAe,CAAC4B,MAAM,CAAC;MAC3DH,QAAQ,CAACE,MAAM,CAAC,UAAU,EAAE,IAAI,CAAC3B,eAAe,CAAC6B,UAAU,CAAC;MAC5DJ,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAAC3B,eAAe,CAAC8B,UAAU,CAAC;MACzDL,QAAQ,CAACE,MAAM,CAAC,QAAQ,EAAE,IAAI,CAAC3B,eAAe,CAAC+B,MAAM,CAAC;MACtDN,QAAQ,CAACE,MAAM,CAAC,UAAU,EAAE,IAAI,CAACvB,QAAQ,CAAC;MAC1CqB,QAAQ,CAACE,MAAM,CAAC,SAAS,EAAE,IAAI