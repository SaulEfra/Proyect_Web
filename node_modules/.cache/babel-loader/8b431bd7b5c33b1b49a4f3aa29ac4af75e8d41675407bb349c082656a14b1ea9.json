{"ast":null,"code":"import axios from 'axios';\nimport Swal from 'sweetalert2';\nexport default {\n  name: 'CerrarCaja',\n  data() {\n    return {\n      datosCajaAbierta: null,\n      montoFinal: '',\n      fechaHoraCierre: ''\n    };\n  },\n  computed: {\n    balanceTotal() {\n      if (this.datosCajaAbierta) {\n        return this.datosCajaAbierta.totalVentas - this.datosCajaAbierta.totalGastos;\n      }\n      return 0;\n    }\n  },\n  async created() {\n    try {\n      const response = await axios.get('http://localhost:3000/CajaAbierta');\n      this.datosCajaAbierta = response.data;\n    } catch (error) {\n      console.error('Error al obtener datos de la caja:', error);\n      Swal.fire({\n        icon: 'error',\n        title: 'Error',\n        text: 'No se pudieron obtener los datos de la caja.'\n      });\n    }\n    this.fechaHoraCierre = new Date().toISOString().slice(0, 16);\n  },\n  methods: {\n    async MontoFinalCaja() {\n      const formData = {\n        montoFinal: this.montoFinal,\n        fechaHoraCierre: this.fechaHoraCierre,\n        IDAbrirCaja: this.datosCajaAbierta.IDAbrirCaja\n      };\n      try {\n        const response = await axios.post('http://localhost:3000/cierrecaja', formData);\n        console.log('Respuesta del servidor:', response.data);\n        await Swal.fire({\n          icon: 'success',\n          title: 'Éxito',\n          text: 'Se cerró caja correctamente',\n          timer: 2000,\n          showConfirmButton: false\n        });\n        this.limpiarDatos();\n        this.$emit('caja-cerrada');\n      } catch (error) {\n        console.error('Error al cerrar caja:', error);\n        Swal.fire({\n          icon: 'error',\n          title: 'Error',\n          text: 'Error al cerrar caja: ' + error.message\n        });\n      }\n    },\n    limpiarDatos() {\n      this.montoFinal = '';\n    }\n  }\n};","map":{"version":3,"names":["axios","Swal","name","data","datosCajaAbierta","montoFinal","fechaHoraCierre","computed","balanceTotal","totalVentas","totalGastos","created","response","get","error","console","fire","icon","title","text","Date","toISOString","slice","methods","MontoFinalCaja","formData","IDAbrirCaja","post","log","timer","showConfirmButton","limpiarDatos","$emit","message"],"sources":["C:\\Users\\Kenny\\Downloads\\Proyect\\Proyect_Web\\src\\components\\CerrarCaja.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <p v-if=\"datosCajaAbierta\">ID Caja: {{ datosCajaAbierta.IDAbrirCaja }}</p>\r\n    <p v-if=\"datosCajaAbierta\">Fecha y Hora: {{ datosCajaAbierta.Fecha }}</p>\r\n    <p v-if=\"datosCajaAbierta\">Monto Inicial: {{ datosCajaAbierta.MontoInicial }}</p>\r\n    <p v-if=\"datosCajaAbierta\">Ventas totales: {{ datosCajaAbierta.totalVentas }}</p>\r\n    <p v-if=\"datosCajaAbierta\">Gastos totales: {{ datosCajaAbierta.totalGastos }}</p>\r\n    <p v-if=\"datosCajaAbierta\">Balance Total: {{ balanceTotal }}</p>\r\n\r\n    <form @submit.prevent=\"MontoFinalCaja\">\r\n      <div class=\"form-group\">\r\n        <label for=\"montoFinal\">Total de dinero en efectivo:</label>\r\n        <input\r\n          type=\"number\"\r\n          id=\"montoFinal\"\r\n          v-model=\"montoFinal\"\r\n          class=\"form-control\"\r\n          placeholder=\"Ingrese el monto final\"\r\n          required\r\n        />\r\n      </div>\r\n\r\n      <button type=\"submit\" class=\"btn btn-primary mt-3\">Cerrar Caja</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport Swal from 'sweetalert2';\r\n\r\nexport default {\r\n  name: 'CerrarCaja',\r\n  data() {\r\n    return {\r\n      datosCajaAbierta: null,\r\n      montoFinal: '',\r\n      fechaHoraCierre: '',\r\n    };\r\n  },\r\n  computed: {\r\n    balanceTotal() {\r\n      if (this.datosCajaAbierta) {\r\n        return (\r\n          this.datosCajaAbierta.totalVentas - this.datosCajaAbierta.totalGastos\r\n        );\r\n      }\r\n      return 0;\r\n    },\r\n  },\r\n  async created() {\r\n    try {\r\n      const response = await axios.get('http://localhost:3000/CajaAbierta');\r\n      this.datosCajaAbierta = response.data;\r\n    } catch (error) {\r\n      console.error('Error al obtener datos de la caja:', error);\r\n      Swal.fire({\r\n        icon: 'error',\r\n        title: 'Error',\r\n        text: 'No se pudieron obtener los datos de la caja.',\r\n      });\r\n    }\r\n    this.fechaHoraCierre = new Date().toISOString().slice(0, 16);\r\n  },\r\n  methods: {\r\n    async MontoFinalCaja() {\r\n      const formData = {\r\n        montoFinal: this.montoFinal,\r\n        fechaHoraCierre: this.fechaHoraCierre,\r\n        IDAbrirCaja: this.datosCajaAbierta.IDAbrirCaja,\r\n      };\r\n      try {\r\n        const response = await axios.post(\r\n          'http://localhost:3000/cierrecaja',\r\n          formData\r\n        );\r\n        console.log('Respuesta del servidor:', response.data);\r\n        \r\n        await Swal.fire({\r\n          icon: 'success',\r\n          title: 'Éxito',\r\n          text: 'Se cerró caja correctamente',\r\n          timer: 2000,\r\n          showConfirmButton: false\r\n        });\r\n\r\n        this.limpiarDatos();\r\n        this.$emit('caja-cerrada');\r\n      } catch (error) {\r\n        console.error('Error al cerrar caja:', error);\r\n        Swal.fire({\r\n          icon: 'error',\r\n          title: 'Error',\r\n          text: 'Error al cerrar caja: ' + error.message,\r\n        });\r\n      }\r\n    },\r\n    limpiarDatos() {\r\n      this.montoFinal = '';\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n  \r\n  <style scoped>\r\n  .form-group {\r\n    margin-bottom: 15px;\r\n  }\r\n  \r\n  .btn-primary {\r\n    background-color: #007bff;\r\n    border-color: #007bff;\r\n    text-align: end;\r\n  }\r\n  \r\n  .btn-primary:hover {\r\n    background-color: #0056b3;\r\n    border-color: #0056b3;\r\n  }\r\n  </style>\r\n  "],"mappings":"AA4BA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAE9B,eAAe;EACbC,IAAI,