{"ast":null,"code":"export default {\n  name: 'CardProduct',\n  props: {\n    title: String,\n    Cantidadprod: Number,\n    descrip: String,\n    presioventa: Number,\n    presioprod: Number,\n    idprod: Number,\n    imagen: String,\n    factu: String,\n    cantidtipo: String,\n    categoriatipo: String\n  },\n  emits: [\"elimin\", \"actu\"],\n  computed: {\n    imageUrl() {\n      return `data:image/jpeg;base64,${this.imagen}`;\n    }\n  },\n  methods: {\n    async usarprop() {\n      this.$emit(\"elimin\", this.idprod);\n    },\n    async usarpropdos() {\n      this.$emit(\"actu\", {\n        id: this.idprod,\n        Categoria: this.categoriatipo,\n        CantidadVenta: this.cantidtipo,\n        Facturas: this.factu,\n        nombre: this.title,\n        cantid: this.Cantidadprod,\n        Desc: this.descrip,\n        Presiovent: this.presioventa,\n        presioprod: this.presioprod,\n        imagen: this.imagen\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","props","title","String","Cantidadprod","Number","descrip","presioventa","presioprod","idprod","imagen","factu","cantidtipo","categoriatipo","emits","computed","imageUrl","methods","usarprop","$emit","usarpropdos","id","Categoria","CantidadVenta","Facturas","nombre","cantid","Desc","Presiovent"],"sources":["C:\\Users\\Kenny\\Downloads\\Proyect\\Proyect_Web\\src\\components\\CardProduct.vue"],"sourcesContent":["<template>\r\n  <div class=\"card\" style=\"width: 18rem;\">\r\n    <div class=\"card-body\">\r\n      \r\n      <img :src=\"imageUrl\" class=\"card-img-top\" alt=\"Product Image\" height=\"200px\" width=\"100px\" style=\"border-radius: 10px;\">\r\n      <p class=\"card-text d-none\">{{ idprod }}</p>\r\n      <h5 class=\"card-title\">{{ title }}</h5>\r\n      <p class=\"card-text\">Precio: ${{ presioventa }}</p>\r\n      <p class=\"card-text\">Cantidad: {{ Cantidadprod }}</p>\r\n\r\n      <!-- Se muestra el mensaje en un estilo de post-it -->\r\n      <p v-if=\"Cantidadprod === 0\" class=\"card-text post-it agotado\">Agotado</p>\r\n      <p v-else-if=\"Cantidadprod < 10\" class=\"card-text post-it bajo-stock\">Bajo stock</p>\r\n\r\n      <p class=\"card-text\">Descripción: {{ descrip }}</p>\r\n      <p class=\"card-text\">Categoría: {{ categoriatipo }}</p>\r\n      <p class=\"card-text\">Cantidad: {{ cantidtipo }}</p>\r\n      \r\n      <div>\r\n          <button class=\"btn-card-elim\" @click=\"usarprop\">Eliminar</button>\r\n          <button type=\"button\" class=\"btn-card\" data-bs-toggle=\"modal\" data-bs-target=\"#actualizarproducto\" @click=\"usarpropdos\">Actualizar Producto</button> \r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'CardProduct',\r\n  props: {\r\n      title: String,\r\n      Cantidadprod: Number,\r\n      descrip: String,\r\n      presioventa: Number,\r\n      presioprod: Number,\r\n      idprod: Number,\r\n      imagen: String,\r\n      factu: String,\r\n      cantidtipo: String,\r\n      categoriatipo:String\r\n  },\r\n  emits: [\"elimin\", \"actu\"],\r\n  computed: {\r\n      imageUrl() {\r\n          return `data:image/jpeg;base64,${this.imagen}`; \r\n      }\r\n  },\r\n\r\n  methods: {\r\n    async usarprop() {\r\n      this.$emit(\"elimin\", this.idprod);\r\n    },\r\n    async usarpropdos() {\r\n      this.$emit(\"actu\", { \r\n        id: this.idprod, \r\n        Categoria: this.categoriatipo, \r\n        CantidadVenta: this.cantidtipo, \r\n        Facturas: this.factu, \r\n        nombre: this.title, \r\n        cantid: this.Cantidadprod, \r\n        Desc: this.descrip, \r\n        Presiovent: this.presioventa, \r\n        presioprod: this.presioprod, \r\n        imagen: this.imagen \r\n      });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .btn-card {\r\n      border: 0px solid;\r\n      height: 40px;\r\n      border-radius: 5px;\r\n      margin: 5px;\r\n      background-color: #d5e4ff;\r\n  }\r\n\r\n  .btn-card:hover {\r\n      background-color: #85a6ff;\r\n      color: white;\r\n  }\r\n\r\n  .btn-card-elim {\r\n      border: 0px solid;\r\n      height: 40px;\r\n      border-radius: 5px;\r\n      margin: 5px;\r\n      background-color: #ffdddd;\r\n  }\r\n\r\n  .btn-card-elim:hover {\r\n      background-color: #ff9494;\r\n      color: white;\r\n  }\r\n\r\n  .card-body {\r\n      text-align: center;\r\n      border: 1px solid rgba(232, 186, 207, 0.7);\r\n      border-radius: 5px;\r\n  }\r\n\r\n  /* Estilo post-it */\r\n  .post-it {\r\n      display: inline-block;\r\n      padding: 10px;\r\n      border-radius: 5px;\r\n      box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);\r\n      font-weight: bold;\r\n      color: white;\r\n  }\r\n\r\n  .agotado {\r\n      background-color: red;\r\n  }\r\n\r\n  .bajo-stock {\r\n      background-color: orange;\r\n  }\r\n</style>\r\n"],"mappings":"AA2BA,eAAe;EACbA,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE;IACHC,KAAK,EAAEC,MAAM;IACbC,YAAY,EAAEC,MAAM;IACpBC,OAAO,EAAEH,MAAM;IACfI,WAAW,EAAEF,MAAM;IACnBG,UAAU,EAAEH,MAAM;IAClBI,MAAM,EAAEJ,MAAM;IACdK,MAAM,EAAEP,MAAM;IACdQ,KAAK,EAAER,MAAM;IACbS,UAAU,EAAET,MAAM;IAClBU,aAAa,EAACV;EAClB,CAAC;EACDW,KAAK,EAAE,CAAC,QAAQ,EAAE,MAAM,CAAC;EACzBC,QAAQ,EAAE;IACNC,QAAQA,CAAA,EAAG;MACP,OAAO,0BAA0B,IAAI,CAACN,MAAM,EAAE;IAClD;EACJ,CAAC;EAEDO,OAAO,EAAE;IACP,MAAMC,QAAQA,CAAA,EAAG;MACf,IAAI,CAACC,KAAK,CAAC,QAAQ,EAAE,IAAI,CAACV,MAAM,CAAC;IACnC,CAAC;IACD,MAAMW,WAAWA,CAAA,EAAG;MAClB,IAAI,CAACD,KAAK,CAAC,MAAM,EAAE;QACjBE,EAAE,EAAE,IAAI,CAACZ,MAAM;QACfa,SAAS,EAAE,IAAI,CAACT,aAAa;QAC7BU,aAAa,EAAE,IAAI,CAACX,UAAU;QAC9BY,QAAQ,EAAE,IAAI,CAACb,KAAK;QACpBc,MAAM,EAAE,IAAI,CAACvB,KAAK;QAClBwB,MAAM,EAAE,IAAI,CAACtB,YAAY;QACzBuB,IAAI,EAAE,IAAI,CAACrB,OAAO;QAClBsB,UAAU,EAAE,IAAI,CAACrB,WAAW;QAC5BC,UAAU,EAAE,IAAI,CAACA,UAAU;QAC3BE,MAAM,EAAE,IAAI,CAACA;MACf,CAAC,CAAC;IACJ;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}