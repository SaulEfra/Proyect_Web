{"ast":null,"code":"import axios from 'axios';\nimport Swal from 'sweetalert2';\nexport default {\n  name: 'AbrirCaja',\n  data() {\n    return {\n      montoInicial: '',\n      fechaHora: ''\n    };\n  },\n  mounted() {\n    this.fechaHora = new Date().toISOString().slice(0, 16);\n  },\n  methods: {\n    validarMonto() {\n      if (this.montoInicial < 0) {\n        this.montoInicial = 0;\n      } else if (this.montoInicial > 50000) {\n        this.montoInicial = 50000;\n      }\n    },\n    async abrirCaja() {\n      try {\n        const formData = {\n          fechaHora: this.fechaHora,\n          montoInicial: this.montoInicial\n        };\n        const openResponse = await axios.post('http://localhost:3000/AbrirCaja', formData, {\n          withCredentials: true\n        });\n        console.log('Respuesta del servidor:', openResponse.data);\n        await Swal.fire({\n          icon: 'success',\n          title: 'Caja Abierta',\n          text: 'Se abrió caja correctamente',\n          timer: 2000,\n          showConfirmButton: false\n        });\n        localStorage.setItem('cajaAbierta', JSON.stringify(openResponse.data));\n        this.limpiarDatos();\n        this.$emit('caja-abierta', openResponse.data);\n      } catch (error) {\n        console.error('Error al abrir caja:', error);\n        Swal.fire({\n          icon: 'error',\n          title: 'Error',\n          text: 'Error al abrir caja: ' + error.message\n        });\n      }\n    },\n    limpiarDatos() {\n      this.montoInicial = '';\n    }\n  }\n};","map":{"version":3,"names":["axios","Swal","name","data","montoInicial","fechaHora","mounted","Date","toISOString","slice","methods","validarMonto","abrirCaja","formData","openResponse","post","withCredentials","console","log","fire","icon","title","text","timer","showConfirmButton","localStorage","setItem","JSON","stringify","limpiarDatos","$emit","error","message"],"sources":["C:\\Users\\Kenny\\Downloads\\Proyect\\Proyect_Web\\src\\components\\AbrirCaja.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <form @submit.prevent=\"abrirCaja\">\r\n      <div class=\"form-group\">\r\n        <label for=\"montoInicial\">Monto Inicial</label>\r\n        <input\r\n          type=\"number\"\r\n          id=\"montoInicial\"\r\n          v-model.number=\"montoInicial\"\r\n          class=\"form-control\"\r\n          placeholder=\"Ingrese el monto inicial\"\r\n          required\r\n          min=\"0\"\r\n          max=\"50000\"\r\n          @input=\"validarMonto\"\r\n        />\r\n      </div>\r\n      <button type=\"submit\" class=\"btn btn-primary mt-3\">Abrir Caja</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport Swal from 'sweetalert2';\r\n\r\nexport default {\r\n  name: 'AbrirCaja',\r\n  data() {\r\n    return {\r\n      montoInicial: '',\r\n      fechaHora: '',\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fechaHora = new Date().toISOString().slice(0, 16);\r\n  },\r\n  methods: {\r\n    validarMonto() {\r\n      if (this.montoInicial < 0) {\r\n        this.montoInicial = 0;\r\n      } else if (this.montoInicial > 50000) {\r\n        this.montoInicial = 50000;\r\n      }\r\n    },\r\n    async abrirCaja() {\r\n      try {\r\n        const formData = {\r\n          fechaHora: this.fechaHora,\r\n          montoInicial: this.montoInicial,\r\n        };\r\n\r\n        const openResponse = await axios.post('http://localhost:3000/AbrirCaja', formData, { withCredentials: true });\r\n        console.log('Respuesta del servidor:', openResponse.data);\r\n        \r\n        await Swal.fire({\r\n          icon: 'success',\r\n          title: 'Caja Abierta',\r\n          text: 'Se abrió caja correctamente',\r\n          timer: 2000,\r\n          showConfirmButton: false\r\n        });\r\n\r\n        localStorage.setItem('cajaAbierta', JSON.stringify(openResponse.data));\r\n\r\n        this.limpiarDatos();\r\n\r\n        this.$emit('caja-abierta', openResponse.data);\r\n      } catch (error) {\r\n        console.error('Error al abrir caja:', error);\r\n        Swal.fire({\r\n          icon: 'error',\r\n          title: 'Error',\r\n          text: 'Error al abrir caja: ' + error.message,\r\n        });\r\n      }\r\n    },\r\n    limpiarDatos() {\r\n      this.montoInicial = '';\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.form-group {\r\n  margin-bottom: 15px;\r\n}\r\n\r\n.btn-primary {\r\n  background-color: #007bff;\r\n  border-color: #007bff;\r\n  text-align: end;\r\n}\r\n\r\n.btn-primary:hover {\r\n  background-color: #0056b3;\r\n  border-color: #0056b3;\r\n}\r\n</style>"],"mappings":"AAuBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,IAAG,MAAO,aAAa;AAE9B,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACD,SAAQ,GAAI,IAAIE,IAAI,CAAC,CAAC,CAACC,WAAW,CAAC,CAAC,CAACC,KAAK,CAAC,CAAC,EAAE,EAAE,CAAC;EACxD,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,IAAI,IAAI,CAACP,YAAW,GAAI,CAAC,EAAE;QACzB,IAAI,CAACA,YAAW,GAAI,CAAC;MACvB,OAAO,IAAI,IAAI,CAACA,YAAW,GAAI,KAAK,EAAE;QACpC,IAAI,CAACA,YAAW,GAAI,KAAK;MAC3B;IACF,CAAC;IACD,MAAMQ,SAASA,CAAA,EAAG;MAChB,IAAI;QACF,MAAMC,QAAO,GAAI;UACfR,SAAS,EAAE,IAAI,CAACA,SAAS;UACzBD,YAAY,EAAE,IAAI,CAACA;QACrB,CAAC;QAED,MAAMU,YAAW,GAAI,MAAMd,KAAK,CAACe,IAAI,CAAC,iCAAiC,EAAEF,QAAQ,EAAE;UAAEG,eAAe,EAAE;QAAK,CAAC,CAAC;QAC7GC,OAAO,CAACC,GAAG,CAAC,yBAAyB,EAAEJ,YAAY,CAACX,IAAI,CAAC;QAEzD,MAAMF,IAAI,CAACkB,IAAI,CAAC;UACdC,IAAI,EAAE,SAAS;UACfC,KAAK,EAAE,cAAc;UACrBC,IAAI,EAAE,6BAA6B;UACnCC,KAAK,EAAE,IAAI;UACXC,iBAAiB,EAAE;QACrB,CAAC,CAAC;QAEFC,YAAY,CAACC,OAAO,CAAC,aAAa,EAAEC,IAAI,CAACC,SAAS,CAACd,YAAY,CAACX,IAAI,CAAC,CAAC;QAEtE,IAAI,CAAC0B,YAAY,CAAC,CAAC;QAEnB,IAAI,CAACC,KAAK,CAAC,cAAc,EAAEhB,YAAY,CAACX,IAAI,CAAC;MAC/C,EAAE,OAAO4B,KAAK,EAAE;QACdd,OAAO,CAACc,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;QAC5C9B,IAAI,CAACkB,IAAI,CAAC;UACRC,IAAI,EAAE,OAAO;UACbC,KAAK,EAAE,OAAO;UACdC,IAAI,EAAE,uBAAsB,GAAIS,KAAK,CAACC;QACxC,CAAC,CA