{"ast":null,"code":"export default {\n  name: 'CardProduct',\n  props: {\n    title: String,\n    Cantidadprod: Number,\n    descrip: String,\n    presioventa: Number,\n    presioprod: Number,\n    idprod: Number,\n    imagen: String\n  },\n  emits: [\"elimin\", \"actu\"],\n  computed: {\n    imageUrl() {\n      return `data:image/jpeg;base64,${this.imagen}`;\n    }\n  },\n  methods: {\n    async usarprop() {\n      this.$emit(\"elimin\", this.idprod);\n    },\n    async usarpropdos() {\n      this.$emit(\"actu\", {\n        id: this.idprod,\n        nombre: this.title,\n        cantid: this.Cantidadprod,\n        Desc: this.descrip,\n        Presiovent: this.presioventa,\n        presioprod: this.presioprod\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","props","title","String","Cantidadprod","Number","descrip","presioventa","presioprod","idprod","imagen","emits","computed","imageUrl","methods","usarprop","$emit","usarpropdos","id","nombre","cantid","Desc","Presiovent"],"sources":["C:\\Users\\Kenny\\Downloads\\Proyect\\Proyect_Web\\src\\components\\CardProduct.vue"],"sourcesContent":["<template>\r\n  <div class=\"card\" style=\"width: 18rem;\">\r\n    \r\n    <div class=\"card-body\">\r\n      <img :src=\"imageUrl\" class=\"card-img-top\" alt=\"Product Image\" height=\"100px\" width=\"100px\">\r\n      {{ imageUrl }}\r\n      <p class=\"card-text d-none\">{{ idprod }}</p>\r\n      <h5 class=\"card-title\">{{ title }}</h5>\r\n      <p class=\"card-text\">Precio: {{ presioventa }} $</p>\r\n      <p class=\"card-text\">Cantidad: {{ Cantidadprod }}</p>\r\n      <p class=\"card-text\">Descripcion: {{ descrip }}</p>\r\n      <div>\r\n          <button class=\"btn-card-elim\" @click=\"usarprop\">Eliminar</button>\r\n          <button type=\"button\" class=\"btn-card \" data-bs-toggle=\"modal\" data-bs-target=\"#actualizarproducto\" @click=\"usarpropdos\">Actualizar Producto</button> \r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\