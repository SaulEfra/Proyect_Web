{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'InicioSesion',\n  data() {\n    return {\n      email: '',\n      password: ''\n    };\n  },\n  methods: {\n    async login() {\n      if (!this.email || !this.password) {\n        alert('Por favor, ingresa tu usuario y contraseña.');\n        return;\n      }\n      try {\n        const response = await axios.post('http://localhost:3000/login', {\n          email: this.email,\n          password: this.password\n        }, {\n          withCredentials: true\n        });\n        if (response.status === 200) {\n          this.$router.push('/Movimientos'); // Redirige al componente Movimientos\n        }\n      } catch (error) {\n        if (error.response) {\n          alert('Error al iniciar sesión: ' + error.response.data);\n        } else if (error.request) {\n          alert('Error al iniciar sesión: No hay respuesta del servidor');\n        } else {\n          alert('Error al iniciar sesión: ' + error.message);\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","email","password","methods","login","alert","response","post","withCredentials","status","$router","push","error","request","message"],"sources":["C:\\Users\\Kenny\\Downloads\\Proyect\\Proyect_Web\\src\\components\\InicioSesion.vue"],"sourcesContent":["<template>\r\n  <div class=\"login\">\r\n    <div class=\"login-container\">\r\n      <h1 class=\"title\">Inicio de sesión</h1>\r\n      <form @submit.prevent=\"login\">\r\n        <div class=\"form-group\">\r\n          <label for=\"user\" class=\"form-label\">Usuario</label>\r\n          <input class=\"form-control\" id=\"user\" v-model=\"email\" type=\"text\" placeholder=\"Ingresa tu usuario\">\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"pass\" class=\"form-label\">Contraseña</label>\r\n          <input class=\"form-control\" id=\"pass\" v-model=\"password\" type=\"password\" placeholder=\"Ingresa tu contraseña\">\r\n        </div>\r\n        <button class=\"login-button\" type=\"submit\">Comenzar</button>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'InicioSesion',\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n    };\r\n  },\r\n  methods: {\r\n    async login() {\r\n      if (!this.email || !this.password) {\r\n        alert('Por favor, ingresa tu usuario y contraseña.');\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const response = await axios.post('http://localhost:3000/login', {\r\n          email: this.email,\r\n          password: this.password\r\n        }, { withCredentials: true });\r\n\r\n        if (response.status === 200) {\r\n          \r\n          this.$router.push('/Movimientos'); // Redirige al componente Movimientos\r\n        }\r\n      } catch (error) {\r\n        if (error.response) {\r\n          alert('Error al iniciar sesión: ' + error.response.data);\r\n        } else if (error.request) {\r\n          alert('Error al iniciar sesión: No hay respuesta del servidor');\r\n        } else {\r\n          alert('Error al iniciar sesión: ' + error.message);\r\n        }\r\n      }\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100vh;\r\n  background: #f7f7f7;\r\n}\r\n\r\n.login-container {\r\n  background: #fff;\r\n  padding: 2rem;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  max-width: 400px;\r\n  width: 100%;\r\n}\r\n\r\n.title {\r\n  text-align: center;\r\n  margin-bottom: 1.5rem;\r\n  font-size: 1.5rem;\r\n  color: #333;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.form-label {\r\n  display: block;\r\n  margin-bottom: 0.5rem;\r\n  font-weight: bold;\r\n}\r\n\r\n.form-control {\r\n  width: 100%;\r\n  padding: 0.75rem;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  font-size: 1rem;\r\n}\r\n\r\n.form-control:focus {\r\n  border-color: #007bff;\r\n  outline: none;\r\n  box-shadow: 0 0 0 0.2rem rgba(38, 143, 255, 0.25);\r\n}\r\n\r\n.login-button {\r\n  width: 100%;\r\n  padding: 0.75rem;\r\n  border: none;\r\n  border-radius: 4px;\r\n  background: #007bff;\r\n  color: #fff;\r\n  font-size: 1rem;\r\n  cursor: pointer;\r\n  transition: background 0.2s ease;\r\n}\r\n\r\n.login-button:hover {\r\n  background: #0056b3;\r\n}\r\n</style>\r\n"],"mappings":";AAoBA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,KAAKA,CAAA,EAAG;MACZ,IAAI,CAAC,IAAI,CAACH,KAAI,IAAK,CAAC,IAAI,CAACC,QAAQ,EAAE;QACjCG,KAAK,CAAC,6CAA6C,CAAC;QACpD;MACF;MAEA,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMR,KAAK,CAACS,IAAI,CAAC,6BAA6B,EAAE;UAC/DN,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,EAAE;UAAEM,eAAe,EAAE;QAAK,CAAC,CAAC;QAE7B,IAAIF,QAAQ,CAACG,MAAK,KAAM,GAAG,EAAE;UAE3B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,cAAc,CAAC,EAAE;QACrC;MACF,EAAE,OAAOC,KAAK,EAAE;QACd,IAAIA,KAAK,CAACN,QAAQ,EAAE;UAClBD,KAAK,CAAC,2BAA0B,GAAIO,KAAK,CAACN,QAAQ,CAACN,IAAI,CAAC;QAC1D,OAAO,IAAIY,KAAK,CAACC,OAAO,EAAE;UACxBR,KAAK,CAAC,wDAAwD,CAAC;QACjE,OAAO;UACLA,KAAK,CAAC,2BAA0B,GAAIO,KAAK,CAACE,OAAO,CAAC;QACpD;MACF;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}