{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'EmpleadosApp',\n  data() {\n    return {\n      mostrarFormulario: false,\n      datosempl: [],\n      empleados: [],\n      nuevoEmpleado: {\n        id: null,\n        Nombre: '',\n        NumeroDeTelefono: '',\n        Rol: '',\n        activo: true\n      },\n      modoEdicion: false,\n      opcionesCargo: ['Encargado', 'Empleado']\n    };\n  },\n  mounted() {\n    this.obtenerEmpleados();\n  },\n  methods: {\n    async agregarEmpleado() {\n      try {\n        if (this.modoEdicion) {\n          const index = this.empleados.findIndex(emp => emp.id === this.nuevoEmpleado.id);\n          if (index !== -1) {\n            this.empleados[index] = {\n              ...this.nuevoEmpleado\n            };\n          }\n          await axios.put(`https://tu-api.com/empleados/${this.nuevoEmpleado.id}`, this.nuevoEmpleado);\n        } else {\n          this.nuevoEmpleado.id = Date.now();\n          await axios.post('http://localhost:3000/Neg/empleados', {\n            id: this.nuevoEmpleado.id,\n            Nombre: this.nuevoEmpleado.Nombre,\n            NumeroDeTelefono: this.nuevoEmpleado.NumeroDeTelefono,\n            Rol: this.nuevoEmpleado.Rol,\n            activo: this.nuevoEmpleado.activo\n          });\n          this.empleados.push({\n            ...this.nuevoEmpleado\n          });\n        }\n        this.resetearFormulario();\n      } catch (error) {\n        console.error(\"Error al enviar los datos del empleado:\", error);\n      }\n    },\n    editarEmpleado(empleado) {\n      this.nuevoEmpleado = {\n        ...empleado\n      };\n      this.modoEdicion = true;\n      this.mostrarFormulario = true;\n    },\n    async borrarEmpleado(id) {\n      if (confirm('¿Está seguro de que desea eliminar este empleado?')) {\n        try {\n          this.empleados = this.empleados.filter(emp => emp.id !== id);\n          // Enviar solicitud DELETE al servidor\n          await axios.delete(`https://tu-api.com/empleados/${id}`);\n        } catch (error) {\n          console.error(\"Error al eliminar el empleado:\", error);\n        }\n      }\n    },\n    resetearFormulario() {\n      this.nuevoEmpleado = {\n        id: null,\n        Nombre: '',\n        NumeroDeTelefono: '',\n        Rol: '',\n        activo: true\n      };\n      this.mostrarFormulario = false;\n      this.modoEdicion = false;\n    },\n    cancelarEdicion() {\n      this.resetearFormulario();\n    },\n    async obtenerEmpleados() {\n      try {\n        const response = await axios.get('http://localhost:3000/Neg/empleadosget');\n        this.datosempl = response.data.results;\n        console.log(this.datosprod);\n      } catch (error) {\n        console.error('Error al obtener los productos:', error);\n        alert('Error al obtener los productos');\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","mostrarFormulario","datosempl","empleados","nuevoEmpleado","id","Nombre","NumeroDeTelefono","Rol","activo","modoEdicion","opcionesCargo","mounted","obtenerEmpleados","methods","agregarEmpleado","index","findIndex","emp","put","Date","now","post","push","resetearFormulario","error","console","editarEmpleado","empleado","borrarEmpleado","confirm","filter","delete","cancelarEdicion","response","get","results","log","datosprod","alert"],"sources":["D:\\Vers_Git_Proyecto\\Proyect_Web\\src\\components\\Empleados.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <nav class=\"navbar bg-body-tertiary\">\r\n      <div class=\"container-fluid\">\r\n        <span class=\"navbar-brand\">\r\n          <h1>Empleados</h1>\r\n        </span>\r\n        <button v-if=\"!mostrarFormulario\" @click=\"mostrarFormulario = true\" class=\"btn btn-primary\">Añadir\r\n          Empleado</button>\r\n      </div>\r\n    </nav>\r\n    <div class=\"row mt-4\">\r\n      <div class=\"col-lg-12\">\r\n        <table v-if=\"!mostrarFormulario\" class=\"table table-striped\">\r\n          <thead>\r\n            <tr>\r\n              <th scope=\"col\">Nombre del empleado</th>\r\n              <th scope=\"col\">Núm. de Teléfono</th>\r\n              <th scope=\"col\">Cargo</th>\r\n              <th scope=\"col\">Acciones</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody class=\"table-group-divider\">\r\n            <tr v-for=\"(empleado, indice) in datosempl\" :key=\"indice\">\r\n              <td>{{ empleado.Nombre }}</td>\r\n              <td>{{ empleado.NumeroDeTelefono }}</td>\r\n              <td>{{ empleado.Cargo }}</td>\r\n              <td>\r\n                <div class=\"btn-group\" role=\"group\">\r\n                  <button class=\"btn btn-secondary\" title=\"Editar\" @click=\"editarEmpleado(empleado)\">\r\n                    <i class=\"fas fa-pencil-alt\"></i>\r\n                  </button>\r\n                  <button class=\"btn btn-danger\" title=\"Eliminar\" @click=\"borrarEmpleado(empleado.id)\">\r\n                    <i class=\"fas fa-trash-alt\"></i>\r\n                  </button>\r\n                </div>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n        <form v-if=\"mostrarFormulario\" @submit.prevent=\"agregarEmpleado\">\r\n          <table class=\"table\">\r\n            <thead>\r\n              <tr>\r\n                <th scope=\"col\">Nombre del empleado</th>\r\n                <th scope=\"col\">Núm. de Teléfono</th>\r\n                <th scope=\"col\">Rol</th>\r\n                <th scope=\"col\">Estado</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody class=\"table-group-divider\">\r\n              <tr>\r\n                <td><input v-model=\"nuevoEmpleado.Nombre\" type=\"text\" required class=\"form-control\"></td>\r\n                <td>\r\n                  <input v-model=\"nuevoEmpleado.NumeroDeTelefono\" type=\"text\" required class=\"form-control\"\r\n                    @input=\"validarTelefono\" pattern=\"[0-9]*\">\r\n                </td>\r\n                <td>\r\n                  <select v-model=\"nuevoEmpleado.Rol\" required class=\"form-select\">\r\n                    <option value=\"\">Seleccione un Rol</option>\r\n                    <option v-for=\"Rol in opcionesCargo\" :key=\"Rol\" :value=\"Rol\">\r\n                      {{ Rol }}\r\n                    </option>\r\n                  </select>\r\n                </td>\r\n                <td>\r\n                  <div class=\"form-check\">\r\n                    <input v-model=\"nuevoEmpleado.activo\" class=\"form-check-input\" type=\"checkbox\" id=\"empleadoActivo\">\r\n                    <label class=\"form-check-label\" for=\"empleadoActivo\">\r\n                      Activo\r\n                    </label>\r\n                  </div>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n          <button type=\"submit\" class=\"btn btn-primary\">{{ modoEdicion ? 'Actualizar' : 'Añadir' }} Empleado</button>\r\n          <button @click=\"cancelarEdicion\" type=\"button\" class=\"btn btn-secondary ml-2\">Cancelar</button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'EmpleadosApp',\r\n  data() {\r\n    return {\r\n      mostrarFormulario: false,\r\n      datosempl: [] ,\r\n      empleados: [],\r\n      nuevoEmpleado: {\r\n        id: null,\r\n        Nombre: '',\r\n        NumeroDeTelefono: '',\r\n        Rol: '',\r\n        activo: true\r\n      },\r\n      modoEdicion: false,\r\n      opcionesCargo: ['Encargado', 'Empleado']\r\n    }\r\n  },\r\n  mounted() {\r\n    this.obtenerEmpleados();\r\n  },\r\n  methods: {\r\n    async agregarEmpleado() {\r\n      try {\r\n        if (this.modoEdicion) {\r\n          const index = this.empleados.findIndex(emp => emp.id === this.nuevoEmpleado.id);\r\n          if (index !== -1) {\r\n            this.empleados[index] = { ...this.nuevoEmpleado };\r\n          }\r\n          await axios.put(`https://tu-api.com/empleados/${this.nuevoEmpleado.id}`, this.nuevoEmpleado);\r\n        } else {\r\n          this.nuevoEmpleado.id = Date.now();\r\n          await axios.post('http://localhost:3000/Neg/empleados', {\r\n            id: this.nuevoEmpleado.id,\r\n            Nombre: this.nuevoEmpleado.Nombre,\r\n            NumeroDeTelefono: this.nuevoEmpleado.NumeroDeTelefono,\r\n            Rol: this.nuevoEmpleado.Rol,\r\n            activo: this.nuevoEmpleado.activo\r\n          });\r\n          this.empleados.push({ ...this.nuevoEmpleado });\r\n        }\r\n        this.resetearFormulario();\r\n      } catch (error) {\r\n        console.error(\"Error al enviar los datos del empleado:\", error);\r\n      }\r\n    },\r\n    editarEmpleado(empleado) {\r\n      this.nuevoEmpleado = { ...empleado };\r\n      this.modoEdicion = true;\r\n      this.mostrarFormulario = true;\r\n    },\r\n    async borrarEmpleado(id) {\r\n      if (confirm('¿Está seguro de que desea eliminar este empleado?')) {\r\n        try {\r\n          this.empleados = this.empleados.filter(emp => emp.id !== id);\r\n          // Enviar solicitud DELETE al servidor\r\n          await axios.delete(`https://tu-api.com/empleados/${id}`);\r\n        } catch (error) {\r\n          console.error(\"Error al eliminar el empleado:\", error);\r\n        }\r\n      }\r\n    },\r\n    resetearFormulario() {\r\n      this.nuevoEmpleado = {\r\n        id: null,\r\n        Nombre: '',\r\n        NumeroDeTelefono: '',\r\n        Rol: '',\r\n        activo: true\r\n      };\r\n      this.mostrarFormulario = false;\r\n      this.modoEdicion = false;\r\n    },\r\n    cancelarEdicion() {\r\n      this.resetearFormulario();\r\n    },\r\n    async obtenerEmpleados(){\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/Neg/empleadosget');\r\n        this.datosempl = response.data.results;\r\n        console.log(this.datosprod);\r\n      } catch (error) {\r\n        console.error('Error al obtener los productos:', error);\r\n        alert('Error al obtener los productos');\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.movements {\r\n  padding: 80px;\r\n  background-color: #fff;\r\n  border-radius: 5px;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n  border-bottom: 1px solid #ccc;\r\n  padding-bottom: 10px;\r\n}\r\n\r\n.employee-table {\r\n  width: 100%;\r\n  background-color: #fff;\r\n  border-radius: 5px;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n  overflow: hidden; /* Asegura que el contenido no desborde */\r\n}\r\n\r\n.table {\r\n  width: 100%;\r\n}\r\n\r\n.table th,\r\n.table td {\r\n  padding: 12px;\r\n  text-align: left;\r\n}\r\n\r\n.table-striped tbody tr:nth-of-type(odd) {\r\n  background-color: rgba(0, 0, 0, 0.05);\r\n}\r\n\r\n.form-check-input {\r\n  margin-top: 0.3rem;\r\n}\r\n\r\n.form-check-label {\r\n  margin-left: 1rem;\r\n}\r\n</style>\r\n"],"mappings":";AAqFA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,iBAAiB,EAAE,KAAK;MACxBC,SAAS,EAAE,EAAC;MACZC,SAAS,EAAE,EAAE;MACbC,aAAa,EAAE;QACbC,EAAE,EAAE,IAAI;QACRC,MAAM,EAAE,EAAE;QACVC,gBAAgB,EAAE,EAAE;QACpBC,GAAG,EAAE,EAAE;QACPC,MAAM,EAAE;MACV,CAAC;MACDC,WAAW,EAAE,KAAK;MAClBC,aAAa,EAAE,CAAC,WAAW,EAAE,UAAU;IACzC;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,gBAAgB,CAAC,CAAC;EACzB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,IAAI,IAAI,CAACL,WAAW,EAAE;UACpB,MAAMM,KAAI,GAAI,IAAI,CAACb,SAAS,CAACc,SAAS,CAACC,GAAE,IAAKA,GAAG,CAACb,EAAC,KAAM,IAAI,CAACD,aAAa,CAACC,EAAE,CAAC;UAC/E,IAAIW,KAAI,KAAM,CAAC,CAAC,EAAE;YAChB,IAAI,CAACb,SAAS,CAACa,KAAK,IAAI;cAAE,GAAG,IAAI,CAACZ;YAAc,CAAC;UACnD;UACA,MAAMN,KAAK,CAACqB,GAAG,CAAC,gCAAgC,IAAI,CAACf,aAAa,CAACC,EAAE,EAAE,EAAE,IA