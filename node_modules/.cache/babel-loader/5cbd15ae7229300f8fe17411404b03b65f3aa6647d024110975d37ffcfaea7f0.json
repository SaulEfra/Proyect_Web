{"ast":null,"code":"import CardProduct from './CardProduct.vue';\nimport AddProductForm from './AddProductForm.vue';\nimport ActProd from './ActProducto.vue';\nimport ParteLateral from '../components/ParteLateral.vue';\nimport axios from 'axios';\nexport default {\n  name: 'ProductInventory',\n  components: {\n    AddProductForm,\n    CardProduct,\n    ActProd,\n    ParteLateral\n  },\n  data() {\n    return {\n      searchQuery: '',\n      categoriaName: '',\n      CantidadProduc: '',\n      datosprod: [],\n      datosprodcount: [],\n      datosprodPreciocount: [],\n      idparaact: []\n    };\n  },\n  computed: {\n    filteredProducts() {\n      return this.datosprod.filter(product => {\n        const lowerCaseQuery = this.searchQuery.toLowerCase();\n        return product.Nombre.toLowerCase().includes(lowerCaseQuery) || product.Descripcion.toLowerCase().includes(lowerCaseQuery);\n      });\n    }\n  },\n  mounted() {\n    this.fetchProductos();\n    this.fetchProductoscount();\n    this.fetchProductosPreciocount();\n  },\n  methods: {\n    async fetchProductos() {\n      try {\n        const response = await axios.get('http://localhost:3000/Productos');\n        this.datosprod = response.data;\n      } catch (error) {\n        console.error('Error al obtener los productos:', error);\n      }\n    },\n    async fetchProductoscount() {\n      try {\n        const response = await axios.get('http://localhost:3000/ProductosCount');\n        this.datosprodcount = response.data;\n      } catch (error) {\n        console.error('Error al obtener el recuento de productos:', error);\n      }\n    },\n    async fetchProductosPreciocount() {\n      try {\n        const response = await axios.get('http://localhost:3000/ProductosPreciocount');\n        this.datosprodPreciocount = response.data;\n      } catch (error) {\n        console.error('Error al obtener el recuento de precios de productos:', error);\n      }\n    },\n    async eliminarProducto(id) {\n      try {\n        await axios.delete(`http://localhost:3000/Productos/${id}`);\n        this.fetchProductos();\n        this.fetchProductoscount();\n        this.fetchProductosPreciocount();\n      } catch (error) {\n        console.error('Error al eliminar el producto:', error);\n      }\n    },\n    actualiProductprueb(id) {\n      this.idparaact = id;\n      const modal = new bootstrap.Modal(document.getElementById('actualizarproducto'));\n      modal.show();\n    },\n    async createCategoriaProd() {\n      try {\n        await axios.post('http://localhost:3000/Categorias', {\n          Nombre: this.categoriaName\n        });\n        this.categoriaName = '';\n      } catch (error) {\n        console.error('Error al crear la categoría:', error);\n      }\n    },\n    async createCantidadProduct() {\n      try {\n        await axios.post('http://localhost:3000/Cantidad', {\n          NombreCantidad: this.CantidadProduc\n        });\n        this.CantidadProduc = '';\n      } catch (error) {\n        console.error('Error al crear la cantidad:', error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["CardProduct","AddProductForm","ActProd","ParteLateral","axios","name","components","data","searchQuery","categoriaName","CantidadProduc","datosprod","datosprodcount","datosprodPreciocount","idparaact","computed","filteredProducts","filter","product","lowerCaseQuery","toLowerCase","Nombre","includes","Descripcion","mounted","fetchProductos","fetchProductoscount","fetchProductosPreciocount","methods","response","get","error","console","eliminarProducto","id","delete","actualiProductprueb","modal","bootstrap","Modal","document","getElementById","show","createCategoriaProd","post","createCantidadProduct","NombreCantidad"],"sources":["C:\\Users\\Kenny\\Downloads\\Proyect\\Proyect_Web\\src\\components\\Productos.vue"],"sourcesContent":["<template>\r\n  <div class=\"row\">\r\n    <div class=\"col-lg-2 col-md-2 col-sm-2 partlt shadow\">\r\n      <ParteLateral />\r\n    </div>\r\n    <div class=\"col-lg-10 col-md-10 col-sm-10\">\r\n      <div class=\"header\">\r\n        <h2 class=\"mb-4 text-center\">Productos</h2>\r\n      </div>\r\n      <div class=\"shadow rounded contProduc col-lg-12 col-md-12 col-sm-12\">\r\n        <div class=\"header-btn\">\r\n          <div class=\"row\">\r\n            <div class=\"col-xl-6 col-lg-9 col-md-12 col-sm-10 col-10 offset-lg-0 offset-md-0 offset-sm-1 offset-1\">\r\n              <input type=\"text\" class=\"form-control inp-btn\" placeholder=\"Buscar producto...\" v-model=\"searchQuery\" />\r\n            </div>\r\n            <div\r\n              class=\"col-xl-2 col-lg-2 col-md-2 col-sm-3 col-4 offset-xl-0 offset-lg-0 offset-md-1 offset-sm-1 offset-3\">\r\n              <button type=\"button\" class=\"btn\" data-bs-toggle=\"modal\" data-bs-target=\"#agregarproducto\">\r\n                Agregar Producto\r\n              </button>\r\n            </div>\r\n            <div\r\n              class=\"col-xl-2 col-lg-2 col-md-2 col-sm-3 col-4 offset-xl-0 offset-lg-2 offset-md-1 offset-sm-1 offset-3\">\r\n              <button type=\"button\" class=\"btn\" data-bs-toggle=\"modal\" data-bs-target=\"#crearcantidad\">\r\n                Crear cantidad\r\n              </button>\r\n            </div>\r\n            <div\r\n              class=\"col-xl-2 col-lg-2 col-md-2 col-sm-3 col-4 offset-xl-0 offset-lg-2 offset-md-1 offset-sm-1 offset-3\">\r\n              <button type=\"button\" class=\"btn\" data-bs-toggle=\"modal\" data-bs-target=\"#crearcategoria\">\r\n                Crear Categoría\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Modal para Agregar Producto -->\r\n        <div class=\"modal fade\" id=\"agregarproducto\" tabindex=\"-1\" aria-labelledby=\"agregarproductoLabel\" aria-hidden=\"true\">\r\n          <div class=\"modal-dialog modal-lg\">\r\n            <div class=\"modal-content\">\r\n              <div class=\"modal-header\">\r\n                <h5 class=\"modal-title\" id=\"agregarproductoLabel\">Agregar Producto</h5>\r\n                <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\r\n              </div>\r\n              <div class=\"modal-body\">\r\n                <AddProductForm @actuprod=\"fetchProductos\" @actuprecio=\"fetchProductosPreciocount\" @actutotalprod=\"fetchProductoscount\" />\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Modal para actualizar Producto -->\r\n        <div class=\"modal fade\" id=\"actualizarproducto\" tabindex=\"-1\" aria-labelledby=\"actualizarproductoLabel\" aria-hidden=\"true\">\r\n          <div class=\"modal-dialog modal-lg\">\r\n            <div class=\"modal-content\">\r\n              <div class=\"modal-header\">\r\n                <h5 class=\"modal-title\" id=\"actualizarproductoLabel\">Actualizar Producto</h5>\r\n                <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\r\n              </div>\r\n              <div class=\"modal-body\">\r\n                <ActProd :idparactuproduc=\"idparaact\" @mostrarprod=\"fetchProductos\" />\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Modal para Crear Categoría -->\r\n        <div class=\"modal fade\" id=\"crearcategoria\" tabindex=\"-1\" aria-labelledby=\"crearcategoriaLabel\" aria-hidden=\"true\">\r\n          <div class=\"modal-dialog modal-lg\">\r\n            <div class=\"modal-content\">\r\n              <div class=\"modal-header\">\r\n                <h5 class=\"modal-title\" id=\"crearcategoriaLabel\">Crear Categoría</h5>\r\n                <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\r\n              </div>\r\n              <div class=\"modal-body\">\r\n                <form @submit.prevent=\"createCategoriaProd\">\r\n                  <label for=\"Nuevcategoria\" class=\"form-label\">Nombre de la categoría</label>\r\n                  <input v-model=\"categoriaName\" class=\"form-control mb-3\" id=\"Nuevcategoria\" type=\"text\" placeholder=\"ejemplo: Artículos de escritura\" required />\r\n                  <div class=\"text-end\">\r\n                    <button class=\"btn btn-primary btn-lg\" type=\"submit\">Crear Categoría</button>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Modal para Crear Cantidad -->\r\n        <div class=\"modal fade\" id=\"crearcantidad\" tabindex=\"-1\" aria-labelledby=\"crearcantidadlabel\" aria-hidden=\"true\">\r\n          <div class=\"modal-dialog modal-lg\">\r\n            <div class=\"modal-content\">\r\n              <div class=\"modal-header\">\r\n                <h5 class=\"modal-title\" id=\"crearcantidadLabel\">Crear Cantidad para los productos</h5>\r\n                <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\r\n              </div>\r\n              <div class=\"modal-body\">\r\n                <form @submit.prevent=\"createCantidadProduct\">\r\n                  <label for=\"Nuevcategoria\" class=\"form-label\">Nombre de la cantidad</label>\r\n                  <input v-model=\"CantidadProduc\" class=\"form-control mb-3\" id=\"Nuevcategoria\" type=\"text\" placeholder=\"ejemplo: Kilos\" required />\r\n                  <div class=\"text-end\">\r\n                    <button class=\"btn btn-primary btn-lg\" type=\"submit\">Crear Cantidad</button>\r\n                  </div>\r\n                </form>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"movements\">\r\n          <div class=\"row\">\r\n            <div class=\"data-pro col-lg-6 col-sm-9 col-12 offset-lg-1 offset-sm-3 offset-2\" v-for=\"dtsprodcoun in datosprodcount\" :key=\"dtsprodcoun\">\r\n              Total de Referencias: {{ dtsprodcoun.cantidadproduct }}\r\n            </div>\r\n            <div class=\"data-pro col-lg-6 col-sm-9 col-12 offset-lg-1 offset-sm-3 offset-2\" v-for=\"dtsprodprecio in datosprodPreciocount\" :key=\"dtsprodprecio\">\r\n              Costo del Inventario: ${{ dtsprodprecio.totalprecio }}\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"karproducto\">\r\n        <CardProduct\r\n          v-for=\"prod in filteredProducts\"\r\n          :key=\"prod.IDProducto\"\r\n          :title=\"prod.Nombre\"\r\n          :Cantidadprod=\"prod.CantidadExistencia\"\r\n          :descrip=\"prod.Descripcion\"\r\n          :imagen=\"prod.Datos\"\r\n          :presioventa=\"prod.PrecioVenta\"\r\n          :presioprod=\"prod.PrecioProduc\"\r\n          :factu=\"prod.Factura\"\r\n          :cantidtipo=\"prod.CantidadVenta\"\r\n          :categoriatipo=\"prod.Categoria\"\r\n          :Codigobarra=\"prod.CodigoBarras\"\r\n          @elimin=\"eliminarProducto\"\r\n          :idprod=\"prod.IDProducto\"\r\n          @actu=\"actualiProductprueb\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport CardProduct from './CardProduct.vue';\r\nimport AddProductForm from './AddProductForm.vue';\r\nimport ActProd from './ActProducto.vue';\r\nimport ParteLateral from '../components/ParteLateral.vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'ProductInventory',\r\n  components: {\r\n    AddProductForm,\r\n    CardProduct,\r\n    ActProd,\r\n    ParteLateral,\r\n  },\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      categoriaName: '',\r\n      CantidadProduc: '',\r\n      datosprod: [],\r\n      datosprodcount: [],\r\n      datosprodPreciocount: [],\r\n      idparaact: [],\r\n    };\r\n  },\r\n  computed: {\r\n    filteredProducts() {\r\n      return this.datosprod.filter((product) => {\r\n        const lowerCaseQuery = this.searchQuery.toLowerCase();\r\n        return (\r\n          product.Nombre.toLowerCase().includes(lowerCaseQuery) ||\r\n          product.Descripcion.toLowerCase().includes(lowerCaseQuery)\r\n        );\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchProductos();\r\n    this.fetchProductoscount();\r\n    this.fetchProductosPreciocount();\r\n  },\r\n  methods: {\r\n    async fetchProductos() {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/Productos');\r\n        this.datosprod = response.data;\r\n      } catch (error) {\r\n        console.error('Error al obtener los productos:', error);\r\n      }\r\n    },\r\n    async fetchProductoscount() {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/ProductosCount');\r\n        this.datosprodcount = response.data;\r\n      } catch (error) {\r\n        console.error('Error al obtener el recuento de productos:', error);\r\n      }\r\n    },\r\n    async fetchProductosPreciocount() {\r\n      try {\r\n        const response = await axios.get('http://localhost:3000/ProductosPreciocount');\r\n        this.datosprodPreciocount = response.data;\r\n      } catch (error) {\r\n        console.error('Error al obtener el recuento de precios de productos:', error);\r\n      }\r\n    },\r\n    async eliminarProducto(id) {\r\n      try {\r\n        await axios.delete(`http://localhost:3000/Productos/${id}`);\r\n        this.fetchProductos();\r\n        this.fetchProductoscount();\r\n        this.fetchProductosPreciocount();\r\n      } catch (error) {\r\n        console.error('Error al eliminar el producto:', error);\r\n      }\r\n    },\r\n    actualiProductprueb(id) {\r\n      this.idparaact = id;\r\n      const modal = new bootstrap.Modal(document.getElementById('actualizarproducto'));\r\n      modal.show();\r\n    },\r\n    async createCategoriaProd() {\r\n      try {\r\n        await axios.post('http://localhost:3000/Categorias', {\r\n          Nombre: this.categoriaName,\r\n        });\r\n        this.categoriaName = '';\r\n      } catch (error) {\r\n        console.error('Error al crear la categoría:', error);\r\n      }\r\n    },\r\n    async createCantidadProduct() {\r\n      try {\r\n        await axios.post('http://localhost:3000/Cantidad', {\r\n          NombreCantidad: this.CantidadProduc,\r\n        });\r\n        this.CantidadProduc = '';\r\n      } catch (error) {\r\n        console.error('Error al crear la cantidad:', error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style scoped>\r\n.header {\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 10px;\r\n  border-bottom: 1px solid #ccc;\r\n  margin-top: 20px;\r\n}\r\n\r\n.movements {\r\n  padding-bottom: 80px;\r\n\r\n}\r\n\r\n.data-pro {\r\n  text-align: center;\r\n  height: 50px;\r\n  width: 300px;\r\n  background-color: #D5E4FF;\r\n  flex-direction: row;\r\n  padding: 10px;\r\n  border-radius: 10px;\r\n  margin-bottom: 20px;\r\n\r\n}\r\n\r\n.btn {\r\n  padding: 10px;\r\n  font-size: 14px;\r\n  border: 1px solid rgb(137, 149, 226);\r\n  border-radius: 5px;\r\n  background-color: #eff5ff;\r\n  cursor: pointer;\r\n  height: 50px;\r\n  width: 150px;\r\n  color: black;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n\r\n.btn:hover {\r\n  background-color: #bdd6ff;\r\n  color: white;\r\n  border: 1px solid rgb(120, 134, 223);\r\n}\r\n\r\n\r\n/* aca empiezan mis estilos */\r\n.btn-close {\r\n  height: 50px;\r\n  width: 50px;\r\n}\r\n\r\n.card {\r\n  margin-top: 20px;\r\n  margin-left: 50px;\r\n}\r\n\r\n.karproducto {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  flex-direction: row;\r\n  margin-top: 20px\r\n}\r\n\r\n.header-btn {\r\n  padding: 20px;\r\n  padding-top: 40px;\r\n  margin-top: 20px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.inp-btn {\r\n  height: 50px;\r\n  margin-bottom: 40px;\r\n}\r\n</style>\r\n"],"mappings":"AA+IA,OAAOA,WAAU,MAAO,mBAAmB;AAC3C,OAAOC,cAAa,MAAO,sBAAsB;AACjD,OAAOC,OAAM,MAAO,mBAAmB